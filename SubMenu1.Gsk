     $SM1_FormA = EditForm($SM1_FormA,"SM1_Browser1","url",$SM1_Url)

     $SM1_CcfMoved=FALSE
     $SM1_CfgNum=Val($_FormLeft)
     IF $SM1_CfgNum > 233
       $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Left",NumToStr($SM1_CfgNum-233))
       $SM1_CcfMoved=TRUE
     ENDIF 
# Now redisplay the modified main form
     WHILE TRUE
       $SM1_FormExit = Form($SM1_FormA,"")
       BEGINCASE 
         CASE $SM1_FormExit="SystemExit"
           BREAK 
         CASE $SM1_FormExit="SM1_ExitColor"
           GOSUB Name=SaveSettings
           BREAK 
         CASE $SM1_FormExit="SM1_UpdateColor"
