   BEGINSUB Name=ReCenterLabel
#    Get the main Form width
     $SM1_Ccontrol = RegExEscape("SM1_Form1")
     $SM1_Property = RegExEscape("Width")
     $SM1_Value  = RegExSub("(?s)$SM1_Ccontrol.*?$SM1_Property.*?=(.*?\r\n)",$SM1_FormA,1,1)
     $SM1_Fwidth  = Val(AllTrim($SM1_Value))
#    Get the Form Id label width
     $SM1_Ccontrol = RegExEscape("SM1_Lbl11")
     $SM1_Property = RegExEscape("Width")
     $SM1_Value = RegExSub("(?s)$SM1_Ccontrol.*?$SM1_Property.*?=(.*?\r\n)",$SM1_FormA,1,1)
     $SM1_LblWidth  = Val(AllTrim($SM1_Value))
#    Calculate a new position for the labels left edge and reposition it.
     $SM1_NewLeft = NumToStr(Int(($SM1_Fwidth/2) - 6.65*(Len($SM1_MfName))))
     $SM1_FormA=EditForm($SM1_FormA,"SM1_Lbl11","Left",$SM1_NewLeft)
   ENDSUB 
