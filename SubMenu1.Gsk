###################################################################################
   BEGINSUB Name=SwapPositions
# 1 - Create macro code dynamically. # 2 - Save the dynamic code to a macro file. # 3 - Run the new macro
     IF $SM1_SwapA <> "0" AND $SM1_SwapB<>"0"
       $SM1_Filedata=""
# 1a - Swap Button captions
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_TempCap=$"+"SM1_F2Bcap"+"$SM1_SwapA"+$_NewLine
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_F2Bcap"+"$SM1_SwapA=$"+"SM1_F2Bcap"+"$SM1_SwapB"+$_NewLine
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_F2Bcap"+"$SM1_SwapB=$"+"SM1_TempCap"+$_NewLine+$_NewLine
# 1b - Swap Description text
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_TempDesc=$"+"SM1_F2Desc"+"$SM1_SwapA"+$_NewLine
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_F2Desc"+"$SM1_SwapA=$"+"SM1_F2Desc"+"$SM1_SwapB"+$_NewLine
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_F2Desc"+"$SM1_SwapB=$"+"SM1_TempDesc"+$_NewLine+$_NewLine
# 1c - Swap File names
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_TempFile=$"+"SM1_F2File"+"$SM1_SwapA"+$_NewLine
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_F2File"+"$SM1_SwapA=$"+"SM1_F2File"+"$SM1_SwapB"+$_NewLine
       $SM1_Filedata=$SM1_Filedata+"$"+"SM1_F2File"+"$SM1_SwapB=$"+"SM1_TempFile"+$_NewLine+$_NewLine
# 2 - Save $SM1_Filedata to a macro file
       $SM1_TempFname="$_Install\Temp\TempMacro.gsk"
       $SM1_TmpStr=PutFile($SM1_TempFname,$SM1_Filedata)
# 3 - Run the new macro
       MACRO File=$SM1_TempFname
     ENDIF 
   ENDSUB 
