   BEGINSUB Name=CfgMfColor                            # Configure Main Form Colors
# Modify the main form to display the browser and new controls
     $SM1_FormA=EditForm($SM1_FormA,"SM1_CfgColorBtn","Visible","No")
     $SM1_FormA=EditForm($SM1_FormA,"SM1_CfgBtn","Visible","No")
