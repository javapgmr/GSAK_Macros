   BEGINSUB Name=InstallColorPicker                                    # Install and unzip the Color Picker file
     $SM1_FileName = $_Install + "\Temp\colorpicker.zip"
     $SM1_ResultStr = PutFile($SM1_FileName,MimeDecode($SM1_Colorpicker))
     IF Left($SM1_ResultStr ,4) <> "*OK*"
       PAUSE Msg="Sorry! For some reason I couldn't generate the colorpicker file"
       CANCEL 
     ELSE 
       $SM1_Folder = $_Install + "\Temp\"
       $SM1_ResultStr = ZipFile("unzip",$SM1_FileName,$SM1_Folder)
       $SM1_Url="file:///" + $_Install + "\Temp\colorpicker.html"
     ENDIF 
   ENDSUB 
