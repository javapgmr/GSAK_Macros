   BEGINSUB Name=Initialise

     $SM1_NewLeft="250"
     $SM1_MfName="Menu Master"

     $SM1_Install="$Gsak$"
     $SM1_Macros="$_Install\Macros"

     MACSETTINGS Type=R FileCheck=N

     GOSUB Name=ReBuildMacroNames

# Restore the form to its last position
     IF $SM1_Fl <> "" AND $SM1_Ft<>""
       $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Top",$SM1_Ft)
       $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Left",$SM1_Fl)
     ENDIF 

     $SM1_Prefix="SM1_"
     $SM1_NewPrefix=""
     $SM1_DefaultStrValue=" "
     $SM1_CtlFile=Sysinfo("MacroPath")+"\MenuMasterCtl.Xml"

# Set Macro Name and Version on the top line of the Form
     $SM1_vN=Left(SysInfo("Macrofile"),AT(".",SysInfo("Macrofile"))-1)
     $SM1_vN=Sysinfo("Macrofile")
     $SM1_vV=AllTrim(RegExSub("macversion *=(.*?)(#|\r\n)",GetFile(SysInfo("MacroPath") + "\" + SysInfo("Macrofile")),1,1))
     $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Caption","MenuMaster  v$SM1_vV - $SM1_vN ($SM1_Prefix)")

# If this macro is Menu Master.Gsk and not a child/clone, display the Update button
     IF $SM1_vN="Menu"+"Master"+".Gsk"
       $SM1_FormB=EditForm($SM1_FormB,"SM1_Updater","Visible","Yes")
     ENDIF 

     GOSUB Name=SetLabelCaptions
     GOSUB Name=SetLabelCS
     GOSUB Name=SetButtonCaptions
     GOSUB Name=ReCenterLabel

   ENDSUB 
