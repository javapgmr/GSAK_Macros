   BEGINSUB Name=RunMacro
     OPTION Explicit=NO

     IF $_AltKey OR FileExists($SM1_Macro) OR At("?",$SM1_Macro)>0
     	 IF $_AltKey  
          IF StrToBool(SysInfo("gsakini;TFMConfig;chkMacroEditor.Checked"))
             $SM1_Editor=SysInfo("gsakini;TFMConfig;edtMacroEditor.FileName")
          ELSE 
             $SM1_Editor="$_exePath\MacroEditor.exe"
          ENDIF 
          IF FileExists($SM1_Macro)       
             RUNPGM Pgm=$SM1_Editor                 # Open the editor - the following delay should allow time to open other macros being edited          	
             DELAY MS=500                         # 1/2 second delay then 
             RUNPGM Pgm=$SM1_Editor Parms=$SM1_Macro  # Pass the name of the macro to the editor
            ELSE
             $Msg="ERROR - The file does not exist"+$_Newline+"$SM1_Macro"
             MSGOK Msg=$Msg Caption="Somebody goofed"
          ENDIF 
     	  ELSE 
     	  	IF $SM1_Vscope AND $_Version >= "7.7.2.37"
     	  	  MACRO File=$SM1_Macro VarScope=Local
     	  	 ELSE 
     	  	 	IF $SM1_Vscope 
     	  	 		$SM1_Msg="This version of Gsak does not support local variables" + $_NewLine + "ALL variables should be considered GLOBAL"
     	  	 		$SM1_Msg=$SM1_Msg+$_newline+$_newline+"To resolve this, update Gsak to v7.7.2.37 or later"
     	  	 		MSGOK Msg=$SM1_Msg Caption="Please Note"
     	  	 	ENDIF 
     	  		MACRO File=$SM1_Macro 
     	  	ENDIF 
        ENDIF 
     ELSE 
       IF $SM1_Macro=""
         $SM1_Msg="There is no macro linked to this button"
       ELSE 
         $SM1_Msg="The macro linked to this button is not available for use."
       ENDIF 
       MSGOK Msg=$SM1_Msg
     ENDIF
      
     OPTION Explicit=YES
     SHOWSTATUS Msg="" Display=OFF
   ENDSUB 
