   BEGINSUB Name=GetRgbColors                 # Return Red/Green/Blue decimal values
     IF Left($SM1_RgbColor,1)="#"                 # $SM1_RgbColor must begin with "#"
       $SM1_Hex=Substr($SM1_RgbColor,2,2)
       GOSUB Name=hextodec
       $SM1_Red=$SM1_Dec
       $SM1_Hex=Substr($SM1_RgbColor,4,2)
       GOSUB Name=hextodec
       $SM1_Green=$SM1_Dec
       $SM1_Hex=Substr($SM1_RgbColor,6,2)
       GOSUB Name=hextodec
       $SM1_Blue=$SM1_Dec
       $SM1_NewColor=NumtoStr(RGBColor($SM1_Red,$SM1_Green,$SM1_Blue))
     ENDIF 
   ENDSUB 
###################################################################################
   BEGINSUB Name=hextodec
     $SM1_HTDSUBnumDigits=len($SM1_Hex)
     IF $SM1_HTDSUBnumDigits = 0
       $SM1_Dec=-1                         # an error condition. no hex characters were passed
     ELSE 
       $SM1_Hex=Upper($SM1_Hex)
       $SM1_Dec=0
       $SM1_HTDSUBhexcodes="0123456789ABCDEF"
       $SM1_HTDSUBcounter=0
       WHILE $SM1_HTDSUBcounter < $SM1_HTDSUBnumDigits
         $SM1_Dec=16*$SM1_Dec + (AT(Substr($SM1_Hex, $SM1_HTDSUBcounter, 1), $SM1_HTDSUBhexcodes)-1)
         $SM1_HTDSUBcounter = $SM1_HTDSUBcounter + 1
       ENDWHILE 
     ENDIF 
   ENDSUB #hextodec
