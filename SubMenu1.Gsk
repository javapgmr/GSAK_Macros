#@##################################################################################
# MacFileName    = SubMenu1.Gsk
# MacVersion     = 1.01
# MacAuthor      = Wayne (Red Roo)
# MacDescription = A simple Menu system for macros with color
# MacUrl         = http://gsak.net/board/index.php?act=ST&f=15&t=13000
#@##################################################################################
# Changes in this version...
#
# Turned off the ShowStatus messagebox when returning to the main form.
# Altered both Swap boxes to display all 11 numbers (0-10) at the same time.
# Made both configuration buttons narrower than other buttons to differentiate them better.
# Pressing <ALT> when selecting a macro to run will attempt to open the macro in your currently configured Macro Editor
# Pressing <ALT>+R when on the Menu (Color Configuration) form will reset the position of the main form.
# Now removes all internal MenuMaster variables beginning with the current variable prefix when MemuMaster closes normally.
# Includes an option to set the scope of ALL variables in a called macro to Local - the default is Global
#
# Notes...
# From any menu - Escape will always access the Exit button
# Option Explicit is always set to No prior to calling a macro
#
###################################################################################

   OPTION Explicit=YES
   VERCHECK Version=7.7.2.37 (Please update GSAK At http://gsak.net)

   GOSUB Name=Initialise

   WHILE TRUE
     $SM1_FormExit = Form($SM1_FormA,"")
     $SM1_Ft=$_FormTop
     $SM1_Fl=$_FormLeft
     $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Top",$SM1_Ft)
     $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Left",$SM1_Fl)
     BEGINCASE 
       CASE $SM1_FormExit="SM1_ExitBtn"
         BREAK 
       CASE $SM1_FormExit = "SystemExit"
         CANCEL 
       CASE $SM1_FormExit = "SM1_CfgColorBtn"
         GOSUB Name=CfgMfColor
       CASE $SM1_FormExit="SM1_Btn01"
         $SM1_Macro=$SM1_F2File1
         $SM1_Vscope=$SM1_Vscope1
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn02"
         $SM1_Macro=$SM1_F2File2
         $SM1_Vscope=$SM1_Vscope2
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn03"
         $SM1_Macro=$SM1_F2File3
         $SM1_Vscope=$SM1_Vscope3
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn04"
         $SM1_Macro=$SM1_F2File4
         $SM1_Vscope=$SM1_Vscope4
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn05"
         $SM1_Macro=$SM1_F2File5
         $SM1_Vscope=$SM1_Vscope5
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn06"
         $SM1_Macro=$SM1_F2File6
         $SM1_Vscope=$SM1_Vscope6
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn07"
         $SM1_Macro=$SM1_F2File7
         $SM1_Vscope=$SM1_Vscope7
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn08"
         $SM1_Macro=$SM1_F2File8
         $SM1_Vscope=$SM1_Vscope8
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn09"
         $SM1_Macro=$SM1_F2File9
         $SM1_Vscope=$SM1_Vscope9
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_Btn10"
         $SM1_Macro=$SM1_F2File10
         $SM1_Vscope=$SM1_Vscope10
         GOSUB Name=RunMacro
       CASE $SM1_FormExit="SM1_CfgBtn"
         $SM1_Ft=$_FormTop
         $SM1_Fl=$_FormLeft
         GOSUB Name=DisplayConfigForm
       CASE $SM1_FormExit="SM1_Freset"                                    # <ALT>+R has been pressed
         $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Top","")
         $SM1_FormA=EditForm($SM1_FormA,"SM1_Form1","Left","")
     ENDCASE 
   ENDWHILE 

   $SM1_Ft=$_FormTop
   $SM1_Fl=$_FormLeft
   GOSUB Name=SaveSettings

   $SM1_BooleanValue = RemoveVar("$SM1_Prefix*")                          # Remove all variables prefixed with this macros' prefix

#  The only variable left now is $line (a string variable created automatically by Gsak during the FileRead process) - which can't be deleted normally.
   
   RETURN 
###################################################################################
############################   End  of  Macro   ###################################
