   BEGINSUB Name=DisplayCloneForm
     $SM1_FormC=EditForm($SM1_FormC,"SM1_CurrentPrefix","Caption",$SM1_Prefix)
     $SM1_FileList=""
     GOSUB Name=ReadCtlFile
     WHILE TRUE
       $SM1_FormExit = Form($SM1_FormC,"")
       BEGINCASE 
         CASE $SM1_FormExit="SM1_PrefixCheck"
           GOSUB Name=CheckPrefix
         CASE $SM1_FormExit="SM1_ExitWith"
           IF $SM1_NewPrefix=""
             MSGOK Msg="You must enter a Variable Prefix for the new macro"
           ELSE 
             IF FolderExists($SM1_NewFolder) AND $SM1_NewName<>""
               IF Lower(Right($SM1_NewName,4))<>".gsk"
                 $SM1_NewName=$SM1_NewName+".Gsk"
               ENDIF 
               $SM1_FullName="$SM1_NewFolder\$SM1_NewName"
               IF At($SM1_FullName,$SM1_FileList)=0
                 $SM1_Cntr=1
                 $SM1_Cntr2=1
                 GOSUB Name=CloneFile
                 $SM1_FileList=$SM1_FileList+"$SM1_NewFolder;$SM1_NewName;$SM1_FullName;$SM1_NewPrefix;$SM1_vV;$_NewLine"
                 GOSUB Name=WriteCtlFile
                 SHOWSTATUS Msg="" Display=OFF
                 MSGOK Msg="MenuMaster has been cloned to $SM1_FullName ($SM1_NewPrefix)"
               ELSE 
                 MSGOK Msg="Sorry - this filename already exists - please choose a different name"
               ENDIF 
             ELSE 
               MSGOK Msg="Please check the Folder name and/or the File name"
             ENDIF 
             BREAK 
           ENDIF 
         CASE $SM1_FormExit="SM1_ExitWithout"
           BREAK 
       ENDCASE 
     ENDWHILE 
   ENDSUB 
###################################################################################
   BEGINSUB Name=CheckPrefix
     IF $SM1_NewPrefix<>"" 
     	IF Lower($SM1_NewPrefix) <> "child"
       IF Lower($SM1_NewPrefix)=Lower($SM1_Prefix) OR RegEx(RegexEscape(";$SM1_NewPrefix;"),$SM1_FileList)
         MSGOK Msg="This prefix has been used previously - use it with caution"
       ELSE 
         MSGOK Msg="This prefix is Ok to use."
       ENDIF 
      ELSE 
      	MSGOK Msg="Sorry - this prefix is Reserved"
      ENDIF
     ELSE 
       MSGOK Msg="Sorry - You must have a variable prefix."
     ENDIF 
   ENDSUB 
###################################################################################
   BEGINSUB Name=CloneFile
     $SM1_OldMacro=Sysinfo("MacroPath")+"\"+Sysinfo("MacroFile")
     $SM1_MacData=GetFile($SM1_OldMacro)
#        $SM1_MacData=Replace($SM1_Prefix,$SM1_NewPrefix,$SM1_MacData)
     $SM1_MacData=""
     $SM1_DataStatement=FALSE
     SHOWSTATUS Msg="Processing $SM1_NewName  ($SM1_Cntr2 of $SM1_Cntr)" Width=350
     FILEREAD File=$SM1_OldMacro
       IF NOT($SM1_DataStatement)
         $line=Replace($SM1_Prefix,$SM1_NewPrefix,$line)
       ENDIF 
       IF RegEx("Data",$line)                                         # Moved down from before 'IF NOT($SM1_DataStatement)' block
         IF RegEx("VarName",$line)
           IF RegEx("colorpicker",$line)
             $SM1_DataStatement=TRUE
           ENDIF 
         ENDIF 
       ENDIF 
       $SM1_MacData=$SM1_MacData+$line+$_NewLine
       IF $SM1_DataStatement AND RegEx(RegexEscape("<EndData>"),$line)
         $SM1_DataStatement=FALSE
       ENDIF 
     ENDREAD 
     $SM1_MacData=Replace($SM1_vN,$SM1_NewName,$SM1_MacData)
     $SM1_ResultStr=PutFile($SM1_FullName,$SM1_MacData)
   ENDSUB 
###################################################################################
   BEGINSUB Name=MacroUpdater            # Note: SM1_Updater button is only displayed on SubMenu1.Gsk
     $SM1_UpdatedFileList=""
     IF FileExists($SM1_CtlFile)
       $SM1_Msg="Re-Clone (update) all Child macros to this version;"
       $SM1_Msg1="Continue and Update all Child Macros"
       $SM1_Msg2="Cancel and exit"
       CHOOSE SM1_Msg=$SM1_Msg Opt1=$SM1_Msg1 Opt2=$SM1_Msg2
       IF $SM1_Result=2
         EXITSUB 
       ENDIF 
       $SM1_FileList=""
       GOSUB Name=ReadCtlFile
       $SM1_Cntr=RegExCount($_NewLine,$SM1_FileList)
       $SM1_Cntr2=1
       WHILE $SM1_Cntr2 <> $SM1_Cntr
         $SM1_Cntr2=$SM1_Cntr2+1
         $SM1_Data       =Extract($SM1_FileList,$_NewLine,$SM1_Cntr2)
         $SM1_NewFolder  =Extract($SM1_Data,";",1)                      # Folder to write new file to
         $SM1_NewName      =Extract($SM1_Data,";",2)                      # New file name
         $SM1_FullName   =Extract($SM1_Data,";",3)                      # Fully qualified file name
         $SM1_NewPrefix  =Extract($SM1_Data,";",4)                      # Prefix for all variables
         $SM1_FileVersion=Extract($SM1_Data,";",5)                      # Version of the file being tested
         IF FileExists($SM1_FullName)
           IF Val($SM1_FileVersion) < Val($SM1_vV)
             $SM1_TempStr=$SM1_NewName
             GOSUB Name=CloneFile
             $SM1_NewFileList=$SM1_NewFileList+"$SM1_NewFolder;$SM1_NewName;$SM1_FullName;$SM1_NewPrefix;$SM1_vV;$_NewLine"
             $SM1_UpdatedFileList=$SM1_UpdatedFileList+$SM1_FullName+$_NewLine
           ELSE 
             $SM1_NewFileList=$SM1_NewFileList+$SM1_Data+$_NewLine
           ENDIF 
         ENDIF 
       ENDWHILE 
       $SM1_FileList=$SM1_NewFileList
       GOSUB Name=WriteCtlFile
       SHOWSTATUS Msg="" Display=OFF
       IF $SM1_UpdatedFileList<>""
         $SM1_Msg="The folowing macros have been updated to version $SM1_vV $_NewLine$_NewLine"+$SM1_UpdatedFileList
         MSGOK Msg=$SM1_Msg
       ENDIF 
     ENDIF 
   ENDSUB 
