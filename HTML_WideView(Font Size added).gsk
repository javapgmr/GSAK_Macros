  $loc = SysInfo("gsakini;LastCenter;Lat") + ", " + SysInfo("gsakini;LastCenter;Lon")
  $h = $h + "<a href='http://maps.google.com/maps?f=d&hl=en&saddr=" + $loc + "&daddr=" + $d_Latitude + ", " + $d_Longitude + "' target='_blank'>Driving Directions</a>"
EndIf

$h = $h + "</td></tr></table><br />"
EndSub

