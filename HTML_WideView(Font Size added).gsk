    $temp = ""
  EndIf

  $h = $h + $temp
EndIf

# Show GC Vote Macro
  if ($showGCVote)
    MACRO File="GCVote_HTML_Include"
    $h = $h + $gcv
  EndIf
  
# End Title Block
$h = $h + "</div><hr />"
