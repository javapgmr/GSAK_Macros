    if $temp<>""
      $temp = $temp + "<br />"
    endIf
    $temp = $temp + "<strong>" + GeoCalc($d_Latitude + ";" + $d_Longitude, "FormatSeconds") + "</strong> (Seconds) "
  endIf

  if ($formatUTM)
    if $temp<>""
      $temp = $temp + "<br />"
    endIf
    $temp = $temp + "<strong>" + GeoCalc($d_Latitude + ";" + $d_Longitude, "FormatUTM") + "</strong> (UTM) "
  endIf

  $temp = replace(";"," ",$temp,true)
  $h = $h + "<br />Coordinates: " + $temp

