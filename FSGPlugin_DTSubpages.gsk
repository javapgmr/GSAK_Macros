	$Out = $Out + "<html>"+$_CrLf
	$Out = $Out + "<head>"+$_CrLf
	$Out = $Out + "<meta http-equiv='Content-Type' content='text/html; charset=utf-8' />"+$_CrLf
	$Out = $Out + "<meta name='keywords' content='Geocaching Statistik FindStatGen Macro GSAK' />"+$_CrLf
	$Out = $Out + "<meta name='description' content='Geocaching - FindStatGen-Plugin FSGPlugin_DTSubpages.gsk by Kappler' />"+$_CrLf
	if $PageDifficulty = -3 and $PageTerrain = -4 
		$Out = $Out + "<title>$Lang02 $LangPlugIn52</title>"+$_CrLf
	endif
	if $PageDifficulty = -3 and $PageTerrain = -3 
		$Out = $Out + "<title>$Lang02 $LangPlugIn51</title>"+$_CrLf
	endif
	if $PageDifficulty = -4 and $PageTerrain = -4 
		$Out = $Out + "<title>$Lang02 $LangPlugIn53</title>"+$_CrLf
	endif
	if $PageDifficulty = -10 and $PageTerrain = -10 
		$Out = $Out + "<title>$Lang28</title>"+$_CrLf
	endif
	if $PageDifficulty = 0 and $PageTerrain = 0 
		$Out = $Out + "<title>$Lang102 $Lang76</title>"+$_CrLf
	endif
	if $PageDifficulty = 0 and $PageTerrain > 0 
		$Out = $Out + "<title>$Lang76 $LangPlugIn21 $Lang28b " + NumToStr($PageTerrain) + "</title>"+$_CrLf
	endif
