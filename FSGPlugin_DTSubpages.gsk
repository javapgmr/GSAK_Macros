		$out = $out + "  <td > </td>" + $_CrLf
	else
		$out = $out + "  <td align='right'><font color='$stColor2c2Hardcore' style='cursor:pointer' onmousedown=" + $_Quote + "document.getElementById('DTM$ActualChart').style.display = 'none';document.getElementById('DTM$NextChart').style.display = 'block';" + $_Quote + ">$LangPlugIn11</font></td>" + $_CrLf
	endif
	
	# extra text-table
	$out = $out + "  <td></td>" + $_CrLf
	$out = $out + "  <td></td>" + $_CrLf
	$out = $out + "</tr>" + $_CrLf
	
	$out = $out + "<tr>" + $_CrLf
	$out = $out + "  <td style='$stTab2cHead' rowspan='10' width='$headerwidth' align='center'><b>$Lang28a</b></td>" + $_CrLf

	$out = $out + "  <td style='$st2c1'></td>" + $_CrLf
	# Get one data line
	$data = Extract($diffterr, ";" , $indexd)
	WHILE $indext < 9
		$indext = $indext + 1
		# Get one result
		$text = Extract($data, ",", $indext)
		$out = $out + "  <td style='$st2c1' width='$cellwidth' align='center'><b>$text</b></td>" + $_CrLf
	ENDWHILE
	# Row Total
	$out = $out + "  <td width='$cellwidth'></td>" + $_CrLf
	$out = $out + "  <td width='90'></td>" + $_CrLf
	$out = $out + "  <td width='120'></td>" + $_CrLf
	$out = $out + "</tr>" + $_CrLf

	$indext = 0
	$indexd = 1

	WHILE $indexd < 10
		$indexd = $indexd + 1
		# Get one data line
		$data = Extract($diffterr, ";" , $indexd)
		$tindexd = replace(",",".",numtoStr($difficulty),true)
		$out = $out + "<tr>" + $_CrLf 
		$out = $out + "  <td style='$st2c1'><b> $tindexd</b></td>" + $_CrLf
		$tmpN = 0
		WHILE $indext < 9
			$indext = $indext + 1
			# Get one result
			$text = Extract($data, ",", $indext)
			$num = Val($text)
			$tmpN = $tmpN + $num
			GOSUB Name=OutputChartLineKappler2
			$terrain = $terrain + 0.5
		ENDWHILE
		# Output Row Total
		$num = $tmpN
		$text = NumToStr($num)
		IF $num >= $maxdif
			$Bold = "<b>"
			$boldoff = "</b>"
			$cellstyle = $stTab2cHead + " color: red;"
			IF ($num > 0)
				$cellstyle = $cellstyle + " cursor:pointer"
			ENDIF
		ELSE
			$bold = ""
			$boldoff = ""
			$cellstyle = $stTab2cHead
			IF ($num > 0)
				$cellstyle = $cellstyle + " cursor:pointer"
			ENDIF
		ENDIF
		IF ($PageDifficulty = $difficulty) and ($PageTerrain = 0)
			$cellstyle = $stDiffTerrActual
		ENDIF
		$out = $out + "  <td style='$cellstyle' align='center'"
		if ($num > 0) and not (($PageDifficulty = $difficulty) and ($PageTerrain = 0))
			$out = $out + " onmousedown=" + $_Quote + "window.location.href='D" + NumToStr($difficulty*10) + "T0.htm';" + $_Quote
		endif
		$out = $out + ">$bold<i>$text</i>$boldoff</td>" + $_CrLf

		$indext = 0
		
		#extra text-table
		begincase
			case $indexd = 4
				# Count caches where difficulty/terrain is greater or equal to 3 as 'Hard' caches
				$_sql = "Select count(Code) from AllFinds WHERE Terrain >=3 OR Difficulty >=3"
				$num = Val(Sqlite("sql",$_sql))
				$tmpN = Int(1000*$num/$FindsWithDuplicates)/10
