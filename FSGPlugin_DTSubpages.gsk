IF $_MacroLevel = 1
	RETURN msg="This macro is a FindStatGen plug-in and cannot be run directly. $_CrLf To use the plugin, add the following line to one of the notes $_CrLf sections in the FindStatsGen macro: $_CrLf $_CrLf <plugin>FSGPlugin_DTSubpages[?AdditionalParameters]</plugin>"
ENDIF

GOSUB name=DeclareVariables
GOSUB Name=InitLanguage

# initialisiere Parameter-Variablen
$SubPagesPath = "empty"
$ChartType = "123"
$ShowChart = 1

# Parameter einlesen
if $_MacroParms <> ""
	$count = RegExCount("&",$_MacroParms)
	$i = 1
	while $i <= $count+1
		$Text1 = extract($_MacroParms,"&",$i)
		$Text2 = upper($Text1)
		
		if (AT("CHARTTYPE", $Text2) <> 0)
			$Text2 = extract($Text1,"=",2)
			if $Text2 <> ""
				$ChartType = AllTrim($Text2)
			else
				RETURN msg="$LangPlugIn01 $_CrLf $Text1"
			endif
		endif
		if (AT("SHOWCHART", $Text2) <> 0)
			$Text2 = extract($Text1,"=",2)
			if $Text2 <> ""
				$ShowChart = val(AllTrim($Text2))
			else
				RETURN msg="$LangPlugIn02 $_CrLf $Text1"
			endif
		endif
		if (AT("SUBPAGESPATH", $Text2) <> 0)
			$Text2 = extract($Text1,"=",2)
			if $Text2 <> ""
				$SubPagesPath = AllTrim($Text2)
				if Right($SubPagesPath, 1) = "\"
					$SubPagesPath = Left($SubPagesPath, Len($SubPagesPath) - 1)
				endif
			else
				RETURN msg="$LangPlugIn03 $_CrLf $Text1"
			endif
		endif
		$i = $i+1
	endwhile
endif

# MsgOK msg="ChartType: '$ChartType' $_CrLf SubPagesPath: '$SubPagesPath' $_CrLf ShowChart: '$ShowChart'"

IF ($SubPagesPath = "empty")
	RETURN msg=$LangPlugIn04
ENDIF
IF RIGHT($SubPagesPath,1) = "\"
	$SubPagesPath = Left($SubPagesPath, Len($SubPagesPath)-1)
ENDIF

# remove Style #4 from ChartType
$ChartType = Replace("4","",$ChartType)

# Initialise Variables
$out = ""

# initialize variable, if necessary, to provide function of notes-testing
if ($FindsWithDuplicates = 0)
	$_sql = "Select * from AllFinds ORDER BY lDate, lLogid"
	$work = Sqlite("sql",$_sql)
	$FindsWithDuplicates = $_SqlRows
endif

GOSUB Name=InitStyles

# Ausgabe-Pfad erstellen, falls er noch nicht existiert
If NOT(FolderExists($SubPagesPath))
	$BatFile = $_Install + "\babel.bat"
	$result = PutFile($BatFile, "MD " + quote($SubPagesPath))
	FileOpen File="$BatFile" Wait=yes
EndIf

# Schleifen über alle DiffTerr-Kombinationen
$PageDifficulty = 1
$PageTerrain  = 1
WHILE $PageDifficulty <= 5
	WHILE $PageTerrain <= 5
		$status = "DTSubPages: Diff/Terr = " + NumToStr($PageDifficulty) + "/" + NumToStr($PageTerrain)
		SHOWSTATUS msg="$status"
		GOSUB name=FindCaches
		IF $NumItems > 0
			GOSUB name=HTMLHeader
			IF $ShowChart = 1
				GOSUB Name=OverviewTable
			ENDIF
			GOSUB name=CacheListe
		
			$Out = UTF8($Out,"e")
			$Out = $Out + "</div></body></html>"+$_CrLf
			$result = PutFile($HTMLFileName,$Out)
		ELSE
			IF FileExists($HTMLFileName)
				FileErase File=$HTMLFileName
			ENDIF
		ENDIF
	
		$PageTerrain = $PageTerrain+0.5
	ENDWHILE
	$PageDifficulty = $PageDifficulty+0.5
	$PageTerrain = 1
ENDWHILE

$PageDifficulty = 1
$PageTerrain = 0
WHILE $PageDifficulty <= 5
	$status = "DTSubPages: Diff/Terr = " + NumToStr($PageDifficulty) + "/ *"
	SHOWSTATUS msg="$status"
	GOSUB name=FindCaches
	IF $NumItems > 0
		GOSUB name=HTMLHeader
		IF $ShowChart = 1
			GOSUB Name=OverviewTable
		ENDIF
		GOSUB name=CacheListe
		
		$Out = UTF8($Out,"e")
		$Out = $Out + "</div></body></html>"+$_CrLf
		$result = PutFile($HTMLFileName,$Out)
	ELSE
		IF FileExists($HTMLFileName)
			FileErase File=$HTMLFileName
		ENDIF
	ENDIF
	
	$PageDifficulty = $PageDifficulty+0.5
ENDWHILE

$PageDifficulty = 0
$PageTerrain  = 1
WHILE $PageTerrain <= 5
	$status = "DTSubPages: Diff/Terr = " + "* /" + NumToStr($PageTerrain)
	SHOWSTATUS msg="$status"
	GOSUB name=FindCaches
	IF $NumItems > 0
		GOSUB name=HTMLHeader
		IF $ShowChart = 1
			GOSUB Name=OverviewTable
		ENDIF
		GOSUB name=CacheListe
		
		$Out = UTF8($Out,"e")
		$Out = $Out + "</div></body></html>"+$_CrLf
		$result = PutFile($HTMLFileName,$Out)
	ELSE
		IF FileExists($HTMLFileName)
			FileErase File=$HTMLFileName
		ENDIF
	ENDIF
	
	$PageTerrain = $PageTerrain+0.5
ENDWHILE

$PageDifficulty = -4
$PageTerrain  = -4
$status = "DTSubPages: Hardcore"
SHOWSTATUS msg="$status"
GOSUB name=FindCaches
IF $NumItems > 0
	GOSUB name=HTMLHeader
	IF $ShowChart = 1
		GOSUB Name=OverviewTable
	ENDIF
	GOSUB name=CacheListe
	$Out = UTF8($Out,"e")
	$Out = $Out + "</div></body></html>"+$_CrLf
	$result = PutFile($HTMLFileName,$Out)
ELSE
	IF FileExists($HTMLFileName)
		FileErase File=$HTMLFileName
	ENDIF
ENDIF

$PageDifficulty = -3
$PageTerrain  = -3
$status = "DTSubPages: Schwere"
SHOWSTATUS msg="$status"
GOSUB name=FindCaches
IF $NumItems > 0
	GOSUB name=HTMLHeader
	IF $ShowChart = 1
		GOSUB Name=OverviewTable
	ENDIF
	GOSUB name=CacheListe
	$Out = UTF8($Out,"e")
	$Out = $Out + "</div></body></html>"+$_CrLf
	$result = PutFile($HTMLFileName,$Out)
ELSE
	IF FileExists($HTMLFileName)
		FileErase File=$HTMLFileName
	ENDIF
ENDIF

$PageDifficulty = -3
$PageTerrain  = -4
$status = "DTSubPages: Extreme"
SHOWSTATUS msg="$status"
GOSUB name=FindCaches
IF $NumItems > 0
	GOSUB name=HTMLHeader
	IF $ShowChart = 1
		GOSUB Name=OverviewTable
	ENDIF
	GOSUB name=CacheListe
	$Out = UTF8($Out,"e")
	$Out = $Out + "</div></body></html>"+$_CrLf
	$result = PutFile($HTMLFileName,$Out)
ELSE
	IF FileExists($HTMLFileName)
		FileErase File=$HTMLFileName
	ENDIF
ENDIF

$PageDifficulty = 0
$PageTerrain  = 0
$Status = "DTSubPages: Diff/Terr = * / *"
SHOWSTATUS msg="$status"
GOSUB name=FindCaches
IF $NumItems > 0
	GOSUB name=HTMLHeader
	IF $ShowChart = 1
		GOSUB Name=OverviewTable
	ENDIF
	GOSUB name=CacheListe
	$Out = UTF8($Out,"e")
	$Out = $Out + "</div></body></html>"+$_CrLf
	$result = PutFile($HTMLFileName,$Out)
ELSE
	IF FileExists($HTMLFileName)
		FileErase File=$HTMLFileName
	ENDIF
ENDIF


