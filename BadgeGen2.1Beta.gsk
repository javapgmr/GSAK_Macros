###---ELEVATION CALCULATIONS---###

BEGINSUB name=CalculateElevation

	# Units?
	IF SysInfo("Distance") = "K"
		$distunits = "m"
	ELSE
		$distunits = "ft"
	ENDIF

	$spaces = "     "

	$conversion = 3.28084

	$outputhtml = $_Install + "\temp\elevationdata.html"

	$database = Replace("'","''",$_CurrentDatabase)

	# Set up the SQL table
	$sqlfile = $_Install + "\macros\elevationdataSQL.db3"

	$status = ""

	IF NOT(FileExists($sqlfile))
		$status = Sqlite("create",$sqlfile)
	ENDIF

	$status = Sqlite("open",$sqlfile)

	$Data="Code text, Database text, Elevation integer, Resolution text, units text, PRIMARY KEY (Code, Database)"
	$_sql = "CREATE TABLE IF NOT EXISTS elevationdata ($Data)"
	$status = Sqlite("sql",$_sql)
	# Filter down to the caches where we have no elevation data
	$Status = DbToSQLite("caches","Code,Name,Country,State,FoundByMeDate,Found",$sqlfile)
	$_sql = "select code from caches WHERE (caches.code NOT IN (SELECT code from elevationdata WHERE database='$EditBox1')) AND Found='T'"
	$codes = Sqlite("sql",$_sql)
	#MSGOK msg="The codes are: $Codes"

	IF NOT($codes="")
		$Sort = $_SortBy
		SORT by="Natural"
		MACROFLAG type=clear range=all
		$status = CodeMatch($codes,$_NewLine,"M")
		MFILTER Expression=$d_MacroFlag
		$TotalCount = $_FilterCount
		$count = 0

		$_sql = "BEGIN"
		$status = Sqlite("sql",$_sql)

		WHILE NOT($_EOL) AND ($TotalCount > 0)
			$count = $count + 1
			$codefield = Replace("'","''",$d_Code)
			$_sql = "SELECT Code from elevationdata WHERE code = '$codefield' and database = '$Database' and elevation <> -32768 and elevation <> -9999"
			$status = Sqlite("sql",$_sql)
			IF Len($status) = 0
				$url = "http://ws5.geonames.org/srtm3?lat=" + $d_Latitude + "&lng=" + $d_Longitude
				SHOWSTATUS msg="Getting 90m resolution Elevation for $d_name" Width=350 Title="$count of $TotalCount"
				$data = GetUrl($url,"")
				$resolution = "90m"
				IF Val($data) = -32768
					$url = "http://ws5.geonames.org/gtopo30?lat=" + $d_Latitude + "&lng=" + $d_Longitude
					SHOWSTATUS msg="Getting 1km resolution Elevation for $d_name" Width=350  Title="$count of $TotalCount"
					$data = GetUrl($url,"")
					$resolution = "1km"
				ENDIF
				IF Len($data) > 0
					$elevation = Val($data)
					IF $elevation = -9999
						$elevation = 0
					ENDIF
					IF $distunits = "ft"
						$elevation = Int($conversion * $elevation)
					ENDIF
					$_sql = "REPLACE INTO elevationdata VALUES('$codefield','$database',$elevation,'$resolution','$distunits')"
					$status = Sqlite("sql",$_sql)
					SHOWSTATUS msg="Posting Elevation for $d_name: $elevation" Width=350  Title="$count of $TotalCount"

				ENDIF
			ENDIF
			GOTO position=next
		ENDWHILE
		SHOWSTATUS msg="End" Width=350 Display=Off

		$_sql = "COMMIT"
		$status = Sqlite("sql",$_sql)

		$SortData = Extract($Sort, "=",1)
		$Sequence = Extract($Sort, "=",2)
		SORT By=$SortData Sequence=$Sequence
	ENDIF
	CANCELFILTER
	#Calculate Maximum Elevation
	$Data = SQLite("Open","$sqlfile")
	$Data = SQLite("sql","Select MAX(elevation) from elevationdata WHERE database='$database' AND code in(SELECT code from Caches WHERE FoundByMeDate<>'')")
	$code = Sqlite("sql","select code from elevationdata where elevation = ($data) AND code in(select code from caches where foundbymedate <>'')limit 1")
	$MaxElevationCache = SQLite("sql","Select name from caches where code = '$code'")

	IF SysInfo("Distance") = "K"
		$Elev = Val($Data)
	ELSE
		$Elev = Round(Val($Data) / $conversion,0)
	ENDIF
	#Calculate Minimum Elevation
	$Data = SQLite("sql","Select MIN(elevation) from elevationdata WHERE database='$database' AND code in(SELECT code from Caches WHERE FoundByMeDate<>'')")
	$code = Sqlite("sql","select code from elevationdata where elevation = ($data) AND code in(select code from caches where foundbymedate <>'')limit 1")
	$MinElevationCache = SQLite("sql","Select name from caches where code = '$code'")

	IF SysInfo("Distance") = "K"
		$Lowe = Val($Data)
	ELSE
		$Lowe = Round(Val($Data) / $conversion,0)
	ENDIF

	IF $Elev<$ElevLIMIT
		$MaxElevationCache=""
	ENDIF
	IF $Lowe>=0
		$MinElevationCache=""
	ENDIF
	$Data = SQLite("Close","","")
ENDSUB #name=CalculateElevation


BEGINSUB name=CalculateDTMatrix
	$CurrentTerrain=1
	$CurrentDifficulty=1
	WHILE Not($CurrentDifficulty=5.5)
		WHILE Not($CurrentTerrain=5.5)
			$Data = Sqlite("sql","Select count(Name) from caches where (terrain = '$CurrentTerrain' AND difficulty='$CurrentDifficulty') AND Found")
			IF Val($Data) > 0
				$MatrixCount = $MatrixCount + 1
			ENDIF
			$CurrentTerrain = $CurrentTerrain + 0.5
		ENDWHILE
		$CurrentDifficulty = $CurrentDifficulty + 0.5
		$CurrentTerrain=1
	ENDWHILE
ENDSUB #CalculateDTMatrix





