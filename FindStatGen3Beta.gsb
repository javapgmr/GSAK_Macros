BEGINSUB Name=CopyToNewDatabase
### Starts Here ###

SHOWSTATUS msg="$LangStatus45" Width=350

IF $_CurrentDatabase <> $Db
	DATABASE Name="$Db" Action=Select
ENDIF

# Any Benchmarks?
$BenchmarkFinds = 0
MFILTER Expression=$d_Found AND $d_CacheType="G"
$BenchmarkFinds=$_FilterCount
CANCELFILTER

	CENTRE Location=$Home
	# Restore SORT from before macro run
	$SORTData = Extract($SavedSortOriginal, "=",1)
	$Sequence = Extract($SavedSortOriginal, "=",2)
	SORT By=$SORTData Sequence=$Sequence

	
SHOWSTATUS msg="$LangStatus46" Width=350

# Copy just the found caches over and ignore Benchmarks
MFILTER Expression=$d_Found AND NOT($d_CacheType="G")

$file = $_Install + "\macros\FoundStatsSQL.txt"

#IF FileExists($file)
#	FILEERASE file=$file
#ENDIF

$Data=""
$Data = "Name;URL;Code;PlacedDate;CacheType;Difficulty;Terrain;FoundByMeDate;DayNumber;OwnerID;OwnerName;State;Country;Distance;Archived;FTF;Container;Latitude;Longitude;Usersort;Logid;Exclude;LogLength;CacheID" + $_NewLine

GOTO position=top

IF Sysinfo("gsakini;TfmConfig;EdtGeoName.Text") = ""
	$msg = "Warning: You don't have a 'Matching Method' set in GSAK." + $CR + $CR + "Please go to Tools/Options and ensure you have set the 'Method for matching caches'." +$CR +$CR+ "You can click 'OK' to continue and the macro will do its best to work it out for you, but note that the macro will miss any caches with multiple finds."
	MSGOK Msg=$msg
ENDIF

$Caches = 0

WHILE NOT($_EOL)

IF Frac($Caches/10)=0
			SHOWSTATUS msg="Processing cache: $caches" Width=350
ENDIF
		
		# Put all the values we need in temporary store
		$LogNumber = $d_FoundCount
		
		# Must be at least one if we got this far
		IF $LogNumber = 0
			$LogNumber = 1
		ENDIF
				
		$C_Code = $d_Code
		$C_Name = $d_Name
		$C_Archived = $d_Archived
		$C_CacheID = $d_CacheID
		$C_CacheType = $d_CacheType
		$C_Container = $d_Container
		$C_Country = $d_Country
		$C_Difficulty = $d_Difficulty
		$C_Distance = $d_Distance
		$C_Latitude = $d_Latitude
		$C_Longitude = $d_Longitude
		$C_OwnerId = $d_OwnerId
		$C_OwnerName = $d_OwnerName
		$C_State = $d_State
		$C_Terrain = $d_Terrain
		$C_Url = $d_Url
		$C_FTF = $d_FTF
		$C_PlacedDate = $d_PlacedDate
		$C_FoundDate = $d_FoundByMeDate
		$C_Exclude = "F"
		
		IF NOT($C_FoundDate > 0)
			$C_FoundDate = [00000000]
		ENDIF
		
		
		IF $d_HasUserNote
			IF RegEx("FindStatsExclude",$d_UserNote)
				$C_Exclude = "T"
			ENDIF
		ENDIF
		
		IF $C_Container = "Not Chosen"
			$C_Container = "Not chosen"
		ENDIF
		
		$CountLogs = 0
		# Go through all Logs
		TABLE Active=Logs Scope=Parent
		WHILE $CountLogs < $LogNumber
			GOTO position=top
			$i = 0
			# Find our find log # $CountLogs  - 1, 2, etc
			#Set catch all date and LogID
			$L_LogDate = $C_FoundDate
			$L_Logid = 0
			WHILE $i <= $CountLogs AND not ($_EoL)
				IF IsOwner() OR $user=$d_lBy
					IF $MyId = 0 AND $d_lOwnerId <>0
						$MyId = $d_lOwnerId
					ENDIF
					$lt = left($d_lType,5)
					IF ($lt="Found" or $lt="Atten" or $lt="Webca")
						$i = $i + 1
						$L_LogDate = $d_lDate
						# $L_Type = $d_lType
						$L_Logid = $d_lLogId
						$L_Log = $d_lText
					ENDIF
				ENDIF
				GOTO Position=next
			ENDWHILE
			
			#Create a new SQL entry and populate it from our stored values. Add a suffix to the code to make it unique.
			
				$CountLogs = $CountLogs + 1
								
				$PlacedDate = DateToSql($C_PlacedDate)
			
				$FoundDate = DateToSql($L_LogDate)
				$datetoconv = $L_LogDate
				GOSUB Name=DayOfWeek
				$tmpS = NumToStr($daynum)
				
				#Force numerics to use the decimal point (not comma) for SQL engine
				$DistanceS = NumToStr(Int(100*$C_Distance))
				$DifficultyS=NumToStr($C_Difficulty)
				$DifficultyS=Replace(",",".",$DifficultyS,TRUE)
				$TerrainS=NumToStr($C_Terrain)
				$TerrainS=Replace(",",".",$TerrainS,TRUE)
				$UsersortS="$d_UserSort"
				$Archived=BoolToStr($C_Archived,"T","F")
				
				#Last log only has the FTF
				IF $CountLogs = $LogNumber
					$FTF=BoolToStr($C_FTF,"T","F")
				ELSE
					$FTF="F"
				ENDIF
				
				$Name=Replace(";",",",$C_Name,TRUE)
				#$Name=Replace("'","",$Name,TRUE)
				$OwnerName=Replace(";",",",$C_OwnerName,TRUE)
				#$OwnerName=Replace("'","",$OwnerName,TRUE)
				$Code = $C_Code + "_" + Str($CountLogs,3,0)
				$Logid = NumToStr($L_Logid)
				
				#Word count
				$Log = NumToStr(RegExCount("\w+",$L_Log))
				
				$Data = $Data + $Name + ";" + $C_Url + ";" + $Code + ";" + $PlacedDate + ";" + $C_CacheType + ";" + $DifficultyS + ";"
				$Data = $Data + $TerrainS + ";" + $FoundDate + ";" + $tmpS + ";" + $C_OwnerId + ";" + $OwnerName + ";" + $C_State + ";" + $C_Country + ";"
				$Data = $Data + $DistanceS + ";" + $Archived + ";" +$FTF + ";" + $C_Container + ";" + $C_Latitude + ";" + $C_Longitude + ";" + $UsersortS + ";" 
				$Data = $Data + $Logid + ";" + $C_Exclude + ";" + $Log + ";" + $C_CacheID + $_NewLine
												
				# $d_CacheID = $C_CacheID
						
		ENDWHILE

		TABLE Active=caches

	GOTO position=next
	$Caches = $Caches + 1
	
ENDWHILE

$Error = PutFile($file,$Data)
	
SHOWSTATUS msg="$LangStatus50" Width=350

ENDSUB 

