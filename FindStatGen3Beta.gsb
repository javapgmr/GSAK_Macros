BEGINSUB Name=DiffTerr
	# Initialise Variables
	$rating  = 1
	$dif     = ""
	$terr    = ""
	$maxdif  = 0
	$maxterr = 0
	WHILE $rating <=5
		# Deal with use of "," as separator
		$trating = replace(",",".",numtoStr($rating),true)
		$text = $trating
		SHOWSTATUS msg="$LangStatus17 $text" Width=350
    $sql = "Select count(Code) from FoundStatsSQL WHERE Difficulty='$trating'"
  	$num = Val(sql($sql,""))
		IF $num>$maxdif
			$maxdif = $num
		ENDIF
		$dif = $dif + "$text,$num;"
		$sql = "Select count(Code) from FoundStatsSQL WHERE Terrain='$trating'"
  	$num = Val(sql($sql,""))
		IF $num>$maxterr
			$maxterr = $num
