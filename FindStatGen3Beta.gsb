BEGINSUB Name=numFastest
#	Determine the time taken for the fastest $numfast finds
#	Initialize Variables
	
#	$numfast = 100

$ThisRecord = 0
$Fastest100=99999

	WHILE $ThisRecord + $numfast <= $FindsWithDuplicates
		$ThisRecord = $ThisRecord + 1
		$ThatRecord = $ThisRecord + $numfast - 1
		$thisdate = SqlToDate(List("dbfound","item","$ThisRecord"))
		$thatdate = SqlToDate(List("dbfound","item","$ThatRecord"))
		$diff = DateDiff($thisdate, $thatdate)
		IF $diff<$fastest100
			$fastest100=$diff
			$f100from = $thisdate
			$f100to = $thatdate
		ENDIF
		IF Frac($Thisrecord/10)=0
			SHOWSTATUS msg="$LangStatus41a $numfast $LangStatus41b $ThisRecord $LangStatus41c $FindsWithDuplicates" Width=350
		ENDIF
		# if this is just one day then force exit early, we can't do better.
		IF $Fastest100 = 0
			$ThisRecord = $FindsWithDuplicates
		ENDIF
	ENDWHILE
	
		
	$fastest100 = $fastest100 + 1
	$text1="$Lang75 $numfast $Lang76"
	IF $fastest100=1
		$text2="<b>$fastest100</b> $Lang77 <b>$f100from</b>"
	ELSE
		$text2="<b>$fastest100</b> $Lang78 <b>$f100from</b> $Lang67 <b>$f100to</b>"
	ENDIF
	GOSUB Name=NumbersLine
ENDSUB #numFastest

BEGINSUB Name=numBestWeekend
	#Initialise variables
	$best = 0
	#Assign first Saturday after first find to $Saturday
	$DatetoConv = $DateFirstFind
	GOSUB Name=DayOfWeek
	$DayOfFirstFind = $dayNum
	$Saturday = $DateFirstFind + (7 - $DayOfFirstFind)
	WHILE $saturday < $_Today
		SHOWSTATUS msg="$LangStatus42 $saturday" Width=350
		$sunday = $saturday + 1
		$saturdaySQL = DateToSql($saturday)
		$sundaySQL = DateToSql($sunday)
		$sql = "SELECT Count(Code) from FoundStatsSQL WHERE FoundByMeDate='$saturdaySQL' OR FoundByMeDate='$sundaySQL'"
		$sqlstring = Sql($sql,"")
		$weekendcount = Val($Sqlstring)
		IF $weekendcount > $best
			$best = $weekendcount
			$bestSaturday = $saturday
			$bestSunday = $sunday
		ENDIF
		$saturday = $saturday + 7
	ENDWHILE
	$text1 = "$Lang79"
	IF $best = 0
		$text2 = "$Lang80"
	ELSE
		$text2 = "<b>$best</b> $Lang81 <b>$bestSaturday</b> $Lang82 <b>$bestSunday</b>"
	ENDIF
	GOSUB Name=NumbersLine
ENDSUB #numBestWeekend

BEGINSUB Name=numBestWeek
	#Initialise variables
	$best = 0
	#Assign first Monday previous to first find to $Monday
	$DatetoConv = $DateFirstFind
	GOSUB Name=DayOfWeek
	$DayOfFirstFind = $dayNum
	$Monday = $DateFirstFind + 2 - $DayOfFirstFind
	$bestMonday = $Monday
	WHILE $monday < $_Today
		SHOWSTATUS msg="$LangStatus43 $monday" Width=350
		$sunday = $monday + 6
		$mondaySQL = DateToSql($monday)
		$sundaySQL = DateToSql($sunday)
		$sql = "SELECT Count(Code) from FoundStatsSQL WHERE FoundByMeDate>='$mondaySQL' AND FoundByMeDate<='$sundaySQL'"
		$sqlstring = Sql($sql,"")
		$weekcount = Val($Sqlstring)
		IF $weekcount > $best
			$best = $weekcount
			$bestMonday	= $monday
		ENDIF
		$monday = $monday + 7
	ENDWHILE
	$bestSunday = $bestMonday + 6
	$text1 = "$Lang83"
	$text2 = "<b>$best</b> $Lang84 <b>$bestMonday</b> $Lang85 <b>$bestSunday</b>"
	GOSUB Name=NumbersLine
ENDSUB #numBestWeek

BEGINSUB Name=numTotalDistance
	#Initialise variables
	
	IF $ExcludeLocationLess
		$note = "<i>$Lang38a</i>"
		$sql = "Select Latitude, Longitude, FoundByMeDate, Logid from FoundStatsSQL WHERE CacheType <>'L' and Exclude <> 'T' ORDER BY FoundByMeDate, #Logid"
	ELSE
		$sql = "Select Latitude, Longitude, FoundByMeDate, Logid from FoundStatsSQL ORDER BY FoundByMeDate, #Logid"
		$note = "<i>$Lang38b</i>"
	ENDIF
	
	$work = sql($sql,"")
	$list = list("sql","replace",$work)
	$count = val(list("sql","count",""))
	$ThisRecord = 0
	#Initialize first coordinates
	$tmpS = list("sql","item","1")
	$last_lat = Extract($tmpS,";",1)
	$last_lon = Extract($tmpS,";",2)
	$Date = Extract($tmpS,";",3)
	$TotalDistance = 0
	$MaxDistance = 0
	$DistanceN = 0

#Here we go
	$tmpN = 1

WHILE $tmpN <= $count
	$ThisRecord = $ThisRecord + 1
	IF Frac($ThisRecord/10) = 0
		SHOWSTATUS msg="$LangStatus44 $ThisRecord" Width=350
	ENDIF
	$tmpS = list("sql","item","$tmpN")
	$latitude = Extract($tmpS,";",1)
	$longitude = Extract($tmpS,";",2)
	$FindDate = Extract($tmpS,";",3)
	$DistData = $last_lat + " " + $last_lon + ";" + $latitude + " " + $longitude
	$DistanceString = GCalc(($DistData),"CalcDistance")
	$DistanceNumeric = Val($DistanceString)
	$TotalDistance = $TotalDistance + $DistanceNumeric
	# Max in one day
	IF $FindDate = $Date
		$DistanceN = $DistanceN + $DistanceNumeric
		IF $DistanceN > $MaxDistance
			$MostInADayDate = SqlToDate($FindDate)
			$MaxDistance = $DistanceN
		ENDIF
	ELSE
		$DistanceN = 0
	ENDIF
	$Date = $FindDate
	$last_lat = $latitude
	$last_lon = $longitude
  $tmpN = $tmpN + 1
ENDWHILE

	$DistanceRounded = Alltrim(Str($TotalDistance,10,0))
	$text1 = "$Lang86"
	$text2 = "<b>$DistanceRounded</b> $distunits, $note"
	GOSUB Name=NumbersLine
	
	# Max in one day
	$DistanceRounded = Alltrim(Str($MaxDistance,10,0))
	$text1 = "$Lang119"
	$text2 = "<b>$DistanceRounded</b> $distunits, $Lang71 <b>$MostInADayDate</b> $note"
	GOSUB Name=NumbersLine
		
ENDSUB #numTotalDistance

