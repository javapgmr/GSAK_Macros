        $CacheType=$d_CacheType
        # Convert Cache Type from Abbreviation to Full Name
        $AbbTypes="ABCELMORTUVWZX"
        $FullTypes="Project Ape;Letterbox;CITO;Event;Locationless;Multi;Other;Earth;Traditional;Unknown/Mystery;Virtual;Webcam;Mega event;GPS Adventures Exhibit;"
        $Position = At($CacheType,$AbbTypes)
        $CacheTypeFull = Extract($FullTypes,";",$Position)
        $CacheDif=NumToStr($d_Difficulty)
        $CacheTer=NumToStr($d_Terrain)
        $CacheStatus="Available"
        
        IF $d_TempDisabled
            $CacheStatus="Temp Disabled"
        ENDIF
        
        IF $d_Archived
            $CacheStatus="Archived"
        ENDIF
        
        IF $d_HasUserNote
					IF RegEx("PlacedStatsAdopted",$d_UserNote)
						$Adopted = "T"
					ENDIF
					IF RegEx("PlacedStatsExclude",$d_UserNote)
						$C_Exclude = "T"
					ENDIF
				ENDIF

        $AgeInDays=DateDiff($d_PlacedDate,$_Today)
        $AgeInMonths=Alltrim(Str($AgeInDays/30.1,4,1))
        GOSUB Name=CountFinds
        IF $PTotalFinds>0
            IF $d_Archived
            	$sql = "RELEASE TABLE temptable"
            	$work = Sql($sql,"")
        			$sql = "ASSIGN TO temptable SELECT * FROM PlacedLogs WHERE lParent = '$d_Code'"
        			$work = Sql($sql,"")
        			$sql = "SELECT lDate FROM temptable WHERE lType = 'Archive' or lType = 'Found it' or lType = 'Webcam Photo Taken' or lType = 'Attended' ORDER by ldate desc"
        			$LogDateDate = SqlToDate(Sql($sql,""))
        			$AgeInDays=DateDiff($d_PlacedDate,$LogDateDate)
        			$AgeInMonths=Alltrim(Str($AgeInDays/30.1,4,1))
        		ENDIF
        		$AvgDaysPerLog=NumToStr(Int(10 * $AgeInDays/$PTotalFinds))
        ELSE
            $AvgDaysPerLog="9998"
        ENDIF     
                
        $Finds=NumToStr($PTotalFinds)
        $lMin = NumToStr($LogMin)
        $lMax = NumToStr($LogMax)
        $lAverage = NumToStr(Int($LogAverage))
        $Data=$Data + $CacheName+";"+$CacheURL+";"+$CacheCode+";"+$PlacedDate+";"+$AgeInMonths+";"+$CacheTypeFull+";"+$CacheDif+";"+$CacheTer+";"+$CacheStatus+";"
        $Data=$Data + $Finds+";"+$AvgDaysPerLog+";"+$lMin+";"+$lMax+";"+$lAverage+";"+$Adopted+";"+$Logid+";"+$C_Exclude+$_NewLine
        #Write data for this cache to the file
        $Error = AppendFile($PlacedStatsData,$Data)
        IF Left($Error,7) = "*Error*"
            Pause Msg="$Error"
            CANCEL
        ENDIF
        GOTO Position=Next
    ENDWHILE
ENDSUB    

      
BEGINSUB Name=CountFinds
    $PTotalFinds=0
    $LogMin = 10000
    $LogMax = 0
    $LogAverage = 0
    TABLE Active=logs Scope=Parent
    GOTO Position=Top
    WHILE NOT($_EOL)
        IF ($d_lType="Found it") OR ($d_lType="Webcam Photo Taken") OR ($d_lType="Attended")
        	  $PTotalFinds=$PTotalFinds+1
				    #Word count
				    $LogLength = RegExCount("\w+",$d_lText)
            IF $LogLength > $LogMax
                $LogMax = $LogLength
                $Logid = NumToStr($d_lLogId)
            ENDIF  
            IF $LogLength < $LogMin
                $LogMin = $LogLength
            ENDIF  
            $LogAverage = $LogAverage + $LogLength
        ENDIF 
        GOTO Position=Next
    ENDWHILE 
    IF $PTotalFinds>0  
        $LogAverage = $LogAverage / $PTotalFinds
    ENDIF
    TABLE Active=Caches
ENDSUB    
 
 
