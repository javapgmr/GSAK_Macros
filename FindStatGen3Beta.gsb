BEGINSUB name=CreateSections
	# Create the stored section order string from the individual drop downs
	$SectionTemp = $S1S + ":" + $S2S + ":" + $S3S + ":" + $S4S + ":" + $S5S + ":" + $S6S + ":" + $S7S + ":" + $S8S + ":" + $S9S + ":" + $S10S + ":" + $S11S + ":" + $S12S + ":" + $S13S + ":" + $S14S + ":" + $S15S + ":" + $S16S + ":" + $S17S + ":" + $S18S
	$SectionOrder = ""
	$count = 1
	WHILE $count <= $NumSections
		$Section = Extract($SectionTemp,":",$count)
		$SectionOrder = $SectionOrder + SubStr($MasterSectionList,At($Section,$MasterSectionList)-3,2) + ":"
		$count = $count + 1
	ENDWHILE
ENDSUB #CreateSections

BEGINSUB Name=ChooseLang
	# Choose language file

	# Get directory listing for language files by running dir and piping it into a text file
	
	$DirFile= $datafilepath + "\FindStatDir.txt"
	$FileMask = $datafilepath + "\FindStatGenLang*.dat"
	
	$BatFile = $_Install + "\babel.bat"
	$result = "dir " + Quote($FileMask) + " /b /o:n >" + Quote($DirFile)
  $result = PutFile($BatFile, $result)
  FILEOPEN File="$BatFile" Wait=yes
	
	$DirList= Getfile($DirFile)

	# Parse Dir listing
	IF NOT(Left($DirList,7) = "*Error*")
		$LineFeed= Chr(13) + chr(10)
		$DirList= Replace($LineFeed, ";" ,$DirList,false)
		$DirList = Left($DirList, Len($DirList)-1)
		$LangList=""
		$LangCount = 1
		$FileName = Extract($DirList,";",$LangCount)
		WHILE NOT($FileName = "")
			$LangFile = $datafilepath + "\$FileName"
			IF (FileExists($LangFile))
				FILEREAD File=$LangFile
				#Language name is in first line after the # then trim whitespace
				$LangName = Extract($line,"#",2)
				$LangName = RegExReplace("^\s+|\s+$", $LangName, "")
				$LangList = $LangList + ";$LangName"
				STOPREAD
				ENDREAD
			ENDIF
			$LangCount = $LangCount + 1
			$FileName = Extract($DirList,";",$LangCount)
		ENDWHILE
	ENDIF

	$LangList = "English" + ";" + "Francais" + ";" + "Deutsch" + $LangList
ENDSUB #Chooselang

