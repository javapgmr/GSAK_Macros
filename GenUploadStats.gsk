################################################################################
# MacVersion = 1.60
# MacDescription = Generate and Upload stats to GC.com
# MacAuthor = Hulkman
# MacFileName = GenUploadStats.gsk
# MacUrl = http://gsak.net/board/index.php?showtopic=10364&view=getlastpost
################################################################################
#
#
#
#store Ctrl key status at startup
PreProcess Ctrl=$ctrlAtStart


#Option Explicit=Yes

Declare Var=$GCProfileUpdate    Type=String
Declare Var=$frmSettings		Type=String
Declare Var=$status             Type=String
Declare Var=$strEXE             Type=String
Declare Var=$strNewFile         Type=String
Declare Var=$strOldFile         Type=String
Declare Var=$strParms           Type=String
Declare Var=$strPassword        Type=String
Declare Var=$strProfileEXE      Type=String
Declare Var=$strUserName        Type=String
Declare Var=$intSettings        Type=Numeric
Declare Var=$strVariables       Type=String
Declare Var=$strResult          Type=String
Declare Var=$intCurrentSettings Type=Numeric
Declare var=$strNewVariables    Type=String
Declare Var=$FormExit			Type=String
Declare Var=$ExternalSettings	Type=Numeric
Declare Var=$strFindStatFile	Type=String
Declare Var=$strSettings	Type=String

If ((NOT($ctrlAtStart) AND FileExists($_Install + "\Macros\GenUploadStats.dat")) OR ($_MacroLevel > 1 AND FileExists($_Install + "\Macros\GenUploadStats.dat")))
	$strVariables = GetFile($_Install + "\Macros\GenUploadStats.dat")
	$strUserName = MimeDecode(Extract($strVariables,$_NewLine,1))
	$strPassword = MimeDecode(Extract($strVariables,$_NewLine,2))
	$strFindStatFile = Extract($strVariables,$_NewLine,3)
	$intSettings = Val(Extract($strVariables,$_NewLine,4))
	$strSettings = Extract($strVariables,$_NewLine,4)
	
	If $strUserName = "" OR $strPassword = "" OR $intSettings = 0 OR $strFindStatFile = ""

		$txtUsername = $strUserName
		$txtPassword = $strPassword
		$cboSetting = $strSettings
		$File1 = $strFindStatFile
	
		$FormExit = form($frmSettings,"")
		
		BeginCase
			Case $FormExit = "SystemExit"
				MsgOk msg="You must set these settings before running the macro.  Exitting..."
				RETURN
			Case $FormExit = "btnCancel"
				MsgOk msg="You must set these settings before running the macro.  Exitting..."
				RETURN
			Case $FormExit = "btnOK"
				$strUserName = $txtUserName
				$strPassWord = $txtPassword
				$intSettings = val($cboSetting)
				
				$strFindStatFile = "$_Install\Macros\FindStatGen3.gsk"
				If $File1 <> "" AND FileExists($File1)
					$strFindStatFile = $File1
				EndIf
				
			$strResult = PutFile($_Install + "\Macros\GenUploadStats.dat", MimeEncode($strUsername)  + MimeEncode($strPassword)  + $strFindStatFile  + $_CrLf + NumToStr($intSettings) ) 
		EndCase
	
	EndIf
	
Else

	$strVariables = GetFile($_Install + "\Macros\GenUploadStats.dat")
	$strUserName = MimeDecode(Extract($strVariables,$_NewLine,1))
	$strPassword = MimeDecode(Extract($strVariables,$_NewLine,2))
	$strFindStatFile = Extract($strVariables,$_NewLine,3)
	$intSettings = Val(Extract($strVariables,$_NewLine,4))
	$strSettings = Extract($strVariables,$_NewLine,4)

	$txtUsername = $strUserName
	$txtPassword = $strPassword
	$cboSetting = $strSettings
	$File1 = $strFindStatFile

		
	$FormExit = form($frmSettings,"")
	
	BeginCase
		Case $FormExit = "SystemExit"
			MsgOk msg="You must set these settings before running the macro.  Exitting..."
			RETURN
		Case $FormExit = "btnCancel"
			MsgOk msg="You must set these settings before running the macro.  Exitting..."
			RETURN
		Case $FormExit = "btnOK"
			$strUserName = $txtUserName
			$strPassWord = $txtPassword
			$intSettings = val($cboSetting)
			
			$strFindStatFile = "$_Install\Macros\FindStatGen3.gsk"
			If $File1 <> "" AND FileExists($File1)
				$strFindStatFile = $File1
			EndIf
			
			$strResult = PutFile($_Install + "\Macros\GenUploadStats.dat", MimeEncode($strUsername)  + MimeEncode($strPassword)  + $strFindStatFile  + $_CrLf + NumToStr($intSettings) ) 
	EndCase
EndIf

#Decode the Upload Utility
$strEXE = $_Install + "\temp\GCProfileUpdate.exe"
$strProfileEXE = MimeDecode($GCProfileUpdate)
$status = PutFile($strEXE,$strProfileEXE)

#Backup the stats file before running FindStatGen
If FileExists($_Install + "\html\stats1.html")
        $strOldFile = $_Install + "\html\stats1.html"
        $strNewFile = $_Install + "\html\OldStats1.html"
        FileCopy From=$strOldFile To=$strNewFile
        FileErase File=$strOldFile
EndIf

#Check to see if the setting group is set, and if not, set it.
$ExternalSettings = $intSettings

#Run FindStatGen
Macro File=$strFindStatFile

#Upload the profile
$strParms = "/s /u" + $_Quote + $strUserName + $_Quote + " /p" + $_Quote + $strPassword + $_Quote
RunPgm pgm="$_Install\temp\GCProfileUpdate.exe" parms=$strParms Wait=Yes

#Remove the exe
FileErase File="$_Install\temp\GCProfileUpdate.exe"

