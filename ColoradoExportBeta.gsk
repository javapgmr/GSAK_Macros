		GOSUB name=DoAttributePOIExport
	ELSE
		GOSUB name=EraseOldAttributePOI
	ENDIF
	
	$AttributeTypeName = "Earthcache"
	IF $AttribEarthcache
		GOSUB name=RestoreFilter
		MFILTER Expression=$d_CacheType="R" Join=AND
		GOSUB name=DoAttributePOIExport
	ELSE
		GOSUB name=EraseOldAttributePOI
	ENDIF
	
	$AttributeTypeName = "Micro"
	IF $AttribMicro
		GOSUB name=RestoreFilter
		MFILTER Expression=$d_Container="Micro" Join=AND
		GOSUB name=DoAttributePOIExport
	ELSE
		GOSUB name=EraseOldAttributePOI
	ENDIF
	
	$AttributeTypeName = "Parking"
	IF $AttribParking
		GOSUB name=RestoreFilter
		$dynamic = $ChildFilter + "cbxCtype2=1"
		MACROSET Dialog=Filter VarName=$dynamic
		FILTER Name=<macro> join=AND
		GOSUB name=DoAttributePOIExport
	ELSE
		GOSUB name=EraseOldAttributePOI
	ENDIF
	
	$AttributeTypeName = "Unavailable"
	IF $AttribUnavailable
		GOSUB name=RestoreFilter
		MFILTER Expression=$d_TempDisabled OR $d_Archived Join=AND
		GOSUB name=DoAttributePOIExport
	ELSE
		GOSUB name=EraseOldAttributePOI
	ENDIF
	
	$AttributeTypeName = "TravelBug"
