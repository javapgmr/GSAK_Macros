BEGINSUB name=POIExport
	IF NOT($ChildAsPoi)
		$POIMessage = ""
		EXITSUB
	ENDIF

	$POIMessage = $_NewLine + "Child points were exported as POIs"
	
	GOSUB name=CreatePOIBMP
	GOSUB name=RestoreFilter
	
	$x = 0

	WHILE $x < $NumChild # Currently we have 6 waypoint types to iterate through
		$ChildTypeName = Extract($ChildTypes,";",$x + 1)
		$dynamic = $ChildFilter + "cbxCtype2=$x"
		$POIFile = $POI_StashPath + "\" + "$GPXFilename" + "_" + $ChildTypeName + ".gpi"
		MACROSET Dialog=Filter VarName=$dynamic
		FILTER Name=<macro> join=AND
		SHOWSTATUS msg="Export Child type: $ChildTypeName" Width=350
		IF $_FilterCount > 0
			# code for POI export
			$POIdynamic = $POIstatic + "fnmTo.Text=$POIFile" + $_NewLine
			IF FileExists("$BMPPath" + "$ChildTypeName" + ".bmp")
				$POIDynamic = $POIDynamic + "iconfile.Text=$BMPPath" + "$ChildTypeName" + ".bmp" + $_NewLine
			ELSE
				$POIDynamic = $POIDynamic + "iconfile.Text=$BMPPath" + "Reference Point.bmp" + $_NewLine
			ENDIF
			$POIDynamic = $POIDynamic + "edtCategory.Text=" + Left($GPXFilename,3) + "_" + "$ChildTypeName" + $_NewLine
			IF $UseCustomName AND RegEx("children",$CustomName)
				$POIDynamic = $POIdynamic + "edtName.Text=" + $CustomName + $_NewLine
			ELSE
				$POIDynamic = $POIdynamic + "edtName.Text=%" + "children %" + "shortname=15-%" + "c_Name" + $_NewLine
			ENDIF

			MACROSET Dialog=GarminPOI VarName=$POIdynamic

			EXPORT type=GPI File=$POIFile Settings=<macro>
			
		ELSE
			#Delete old file if there isn't a new one to overwrite
			FILEERASE File=$POIFile OnError=Continue
		ENDIF
		
		# Delete any old files left in the main POI folder
		$POIFile = $POIPath + "\" + "$GPXFilename" + "_" + $ChildTypeName + ".gpi"
		FILEERASE File=$POIFile OnError=Continue
		
		GOSUB name=RestoreFilter
		$x = $x + 1
	ENDWHILE
	
	# Clear child flags
	
	MACROSET Dialog=Filter VarName=$ClearChildFlagsFilter
	FILTER Name=<macro> join=AND
	
	GOSUB name=RestoreFilter
	
	SHOWSTATUS Msg="Done" Display=off

ENDSUB #POIExport

