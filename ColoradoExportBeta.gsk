BEGINSUB name=FoundPOIExport
	IF NOT($FoundAsPoi)
		$FoundPOIMessage = ""
		EXITSUB
	ENDIF

	$FoundPOIMessage = $_NewLine + "Found caches were exported as POIs"

	# Write out BMP files for the POI icon
	$BMPPath = $_Install + "\temp\"
	$tmpS = PutFile($BMPPath + "Found.bmp", MimeDecode($Found))
	$tmpS = PutFile($BMPPath + "FoundDisabled.bmp", MimeDecode($FoundDisabled))

	# Write out POI description macro
	$tmpS = PutFile($_Install + "\macros\FoundPOIDescription.gsk", $FoundPOIDescription)

	$POIPath = $Drive + ":\Garmin\POI"

	# Create POI directory if it doesn't exist
	IF FolderExists ($POIpath) = FALSE
		$comspec = GetEnvV("comspec")
		RUNPGM pgm=$comspec parms=/C md "$POIPath" wait=yes Hide=Yes
	ENDIF

	# Change database if necessary
	IF $_CurrentDatabase <> $FoundDatabase
		DATABASE Name="$FoundDatabase" Action=Select
	ENDIF
	
	# Open the SQLite database to allow interrogation of the logs
	$sqlinputfile = SqlQuote($_CurrentDataPath + "\sqlite.db3")
	$_sql = "ATTACH $sqlinputfile AS foundgsakdb"
	$status = Sqlite("sql",$_sql)

	# Apply the Filters for Found Disabled
	IF $FoundFilter = "NONE"
		MFILTER Expression=TRUE
	ELSE
		FILTER Name=$FoundFilter
	ENDIF
	IF $_FilterCount > 0
		MFILTER Expression=$d_Found AND ($d_TempDisabled OR $d_Archived) Join=AND
		IF $_FilterCount > 0
			# code for POI export
			SHOWSTATUS msg="Export Disabled/Archived Found Caches as POIs" Width=350
			$FoundPOIFile = $POIPath + "\" + "$FoundDatabase" + "_" + "FoundDisabled.gpi"
			$FoundPOIdynamic = $FoundPOIstatic + "fnmTo.Text=$POIFile" + $_NewLine
			$FoundPOIDynamic = $FoundPOIDynamic + "iconfile.Text=$BMPPath" + "FoundDisabled.bmp" + $_NewLine
			$FoundPOIDynamic = $FoundPOIDynamic + "edtCategory.Text=Found - Unavailable" + $_NewLine
			
			IF $UseCustomFoundName
				$FoundPOIDynamic = $FoundPOIDynamic + "edtName.Text=" + $CustomFoundName + $_NewLine
			ELSE
				$FoundPOIDynamic = $FoundPOIDynamic + "edtName.Text=" +  "%" + "Name by " + "%" +"by " + "%" + "typ1" + "%" + "con1 (" + "%" + "Dif/" + "%" + "Ter)" + $_NewLine
			ENDIF
			
			MACROSET Dialog=GarminPOI VarName=$FoundPOIdynamic
			EXPORT type=GPI File=$FoundPOIFile Settings=<macro>

		ENDIF
	ENDIF
	
		# Apply the Filters for Found Active
	IF $FoundFilter = "NONE"
		MFILTER Expression=TRUE
	ELSE
		FILTER Name=$FoundFilter
	ENDIF
	IF $_FilterCount > 0
		MFILTER Expression=$d_Found AND Not($d_TempDisabled OR $d_Archived) Join=AND
		IF $_FilterCount > 0
			# code for POI export
			SHOWSTATUS msg="Export Active Found Caches as POIs" Width=350
			$FoundPOIFile = $POIPath + "\" + "$FoundDatabase" + "_" + "Found.gpi"
			$FoundPOIdynamic = $FoundPOIstatic + "fnmTo.Text=$POIFile" + $_NewLine
			$FoundPOIDynamic = $FoundPOIDynamic + "iconfile.Text=$BMPPath" + "Found.bmp" + $_NewLine
			$FoundPOIDynamic = $FoundPOIDynamic + "edtCategory.Text=Found" + $_NewLine
			
			IF $UseCustomFoundName
				$FoundPOIDynamic = $FoundPOIDynamic + "edtName.Text=" + $CustomFoundName + $_NewLine
			ELSE
				$FoundPOIDynamic = $FoundPOIDynamic + "edtName.Text=" +  "%" + "Name by " + "%" +"by " + "%" + "typ1" + "%" + "con1 (" + "%" + "Dif/" + "%" + "Ter)" + $_NewLine
			ENDIF
			
			MACROSET Dialog=GarminPOI VarName=$FoundPOIdynamic
			EXPORT type=GPI File=$FoundPOIFile Settings=<macro>

		ENDIF
	ENDIF
	
	GOSUB name=FoundChildPOIExport
	
	# Close SQLite database again
	$_sql = "DETACH foundgsakdb"
	$status = Sqlite("sql",$_sql)
	
	# return to original database
	IF $_CurrentDatabase <> $currentdb
		DATABASE Name="$currentdb" Action=Select
	ENDIF

	SHOWSTATUS Msg="Done" Display=off

ENDSUB #FoundPOIExport

BEGINSUB name=PlacedPOIExport
	IF NOT($PlacedAsPoi)
		$PlacedPOIMessage = ""
		EXITSUB
	ENDIF

	$PlacedPOIMessage = $_NewLine + "Placed caches were exported as POIs"

	# Write out BMP files for the POI icon
	$BMPPath = $_Install + "\temp\"
	$tmpS = PutFile($BMPPath + "Placed.bmp", MimeDecode($Placed))

	# Write out POI description macro
	$tmpS = PutFile($_Install + "\macros\POIDescription.gsk", $POIDescription)

	$POIPath = $Drive + ":\Garmin\POI"

	# Create POI directory if it doesn't exist
	IF FolderExists ($POIpath) = FALSE
		$comspec = GetEnvV("comspec")
		RUNPGM pgm=$comspec parms=/C md "$POIPath" wait=yes Hide=Yes
	ENDIF

	# Change database if necessary
	IF $_CurrentDatabase <> $PlacedDatabase
		DATABASE Name="$PlacedDatabase" Action=Select
	ENDIF

	# Apply the Filters
	IF $PlacedFilter = "NONE"
		MFILTER Expression=TRUE
	ELSE
		FILTER Name=$PlacedFilter
	ENDIF
	IF $_FilterCount > 0
		MFILTER Expression=IsOwner() Join=AND
		IF $_FilterCount > 0
			# code for POI export
			SHOWSTATUS msg="Export Placed Caches as POIs" Width=350
			$PlacedPOIFile = $POIPath + "\" + "$PlacedDatabase" + "_" + "Placed.gpi"
			$PlacedPOIdynamic = $PlacedPOIstatic + "fnmTo.Text=$POIFile" + $_NewLine
			$PlacedPOIdynamic = $PlacedPOIdynamic + "iconfile.Text=$BMPPath" + "Placed.bmp" + $_NewLine
			$PlacedPOIdynamic = $PlacedPOIdynamic + "edtCategory.Text=Placed" + $_NewLine
			
			IF $UseCustomPlacedName
				$PlacedPOIdynamic = $PlacedPOIdynamic + "edtName.Text=" + $CustomPlacedName + $_NewLine
			ELSE
				$PlacedPOIdynamic = $PlacedPOIdynamic + "edtName.Text=" +  "%" + "Name by " + "%" +"by " + "%" + "typ1" + "%" + "con1 (" + "%" + "Dif/" + "%" + "Ter)" + $_NewLine
			ENDIF
			
			MACROSET Dialog=GarminPOI VarName=$PlacedPOIdynamic
			EXPORT type=GPI File=$PlacedPOIFile Settings=<macro>

		ENDIF
	ENDIF
	
	GOSUB name=PlacedChildPOIExport
	
	# return to original database
	IF $_CurrentDatabase <> $currentdb
		DATABASE Name="$currentdb" Action=Select
	ENDIF

	SHOWSTATUS Msg="Done" Display=off

ENDSUB #PlacedPOIExport

