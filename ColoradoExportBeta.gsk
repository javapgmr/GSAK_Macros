BEGINSUB name=SelectAttributes
$_sql= "Attach " + SqlQuote("$_ExePath\static.db3") + " as static"
$work=sqlite("sql",$_sql,"")
 
$_sql = "select vfrom, vto, d1 from lookup where type = 'attributes' order by vto" 
$work = Sqlite("sql",$_sql)
$count = $_sqlrows
$i = 1

$_sql = "DETACH static"
$tmpS = Sqlite("sql",$_sql)

$Attribform = $formstatic
$offset = 10
$top = 70

WHILE $i <= $count
	$line = Extract($work,$_NewLine,$i)
	$AttribNumber = Extract($line,";",1)
	$AttribName = Extract($line,";",2)
	$AttribNone = Extract($line,";",3)
	$tmpS = $formitem
	$tmps = Replace("%%",$AttribNumber,$tmpS)
	$tmpS = Replace("!!name!!",$AttribName,$tmpS)
	$tmpS = Replace("!!leftlabel!!","$offset",$tmpS)
	$tmpS = Replace("!!top!!","$top",$tmpS)
	
	$tmpN = $offset + 140
	$tmpS = Replace("!!left1!!","$tmpN",$tmpS)
	
	$tmpN = $offset + 175
	$tmpS = Replace("!!left2!!","$tmpN",$tmpS)
	IF $AttribNone <> "N"
		$tmpS = Replace("!!visible!!","Yes",$tmpS)
	ELSE
		$tmpS = Replace("!!visible!!","No",$tmpS)
	ENDIF
	
	$tmpN = $offset + 210
	$tmpS = Replace("!!left3!!","$tmpN",$tmpS)
	
	$Attribform = $Attribform + $tmpS
		
	$top = $top + 17
	IF $i = Int(0.5 + $count/2)
		$top = 70
		$offset = 280
	ENDIF
	$i = $i + 1
ENDWHILE

$AttribAll = TRUE
$tmpS = Form($Attribform,"")

ENDSUB #SelectAttributes

