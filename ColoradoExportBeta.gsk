BEGINSUB name=ConvertWaypoints
	#DEBUG status=on
	# This checks for entries where the cache id = 1 and converts them to waypoints

	$GPX = ""				# Final output
	$Caches = 0
	$Wpts = 0

	# Get file and strip out the <groundspeak> tags
	
	SHOWSTATUS msg="Loading $GPXFilename gpx file for processing" Width=350

	$GPX = GetFile($GPXFile)
	IF Left($GPX,7) = "*Error*"
		PAUSE Msg="$GPX"
		RETURN
	ENDIF
	
	SHOWSTATUS Msg="Done" Display=off
	
	# Convert to large characters if needed
	IF $UseBigCharacters
		# Short and long descriptions
		$tmpS = "description html=" + Quote("True") + ">"
		$GPX = Replace($tmpS,$tmpS + "&lt;big&gt;",$GPX)
		$tmpS = "description html=" + Quote("False") + ">"
		$GPX = Replace($tmpS,$tmpS + "&lt;big&gt;",$GPX)
		
		$tmpS = "</groundspeak:short_description>"
		$GPX = Replace($tmpS,"&lt;/big&gt;" + $tmpS,$GPX)
		$tmpS = "</groundspeak:long_description>"
		$GPX = Replace($tmpS,"&lt;/big&gt;" + $tmpS,$GPX)
		
		# Logs
		$tmpS = "<groundspeak:text encoded=" + Quote("True") + ">"
		$GPX = Replace($tmpS,$tmpS + "&lt;big&gt;",$GPX)
		$tmpS = "<groundspeak:text encoded=" + Quote("False") + ">"
		$GPX = Replace($tmpS,$tmpS + "&lt;big&gt;",$GPX)
		
		$tmpS = "</groundspeak:text>"
		$GPX = Replace($tmpS,"&lt;/big&gt;" + $tmpS,$GPX)
	ENDIF
		
#	# Remove bb code from logs 
#	$GPX = RegExReplace("\[(|/)(\w|=|#|/|\.)*?\]",$GPX,"")
#		
#	# Remove any remaining [ and ] to leave the smileys as ASCII
#	$GPX = RegExReplace("\[|\]",$GPX,"")
#	$result = PutFile($GPXFile, $GPX)
			
	# Main loop

	# Check if we have any points with cacheid = 1, if so convert them to waypoints
	$trigger = "cache id=" + Quote("1")
	IF (RegExCount($trigger,$GPX)) =0
		IF $UseBigCharacters
			$result = PutFile($GPXFile, $GPX)
			IF Left($result, 7) = "*Error*"
				# If theres any error abort the macro
			CANCEL Msg="Unable to write to $file!"
			ENDIF
		ENDIF
		IF $Silent = FALSE
			# Get specific counts of geocaches and child waypoints
			$Wpts = RegExCount("<type>Waypoint",$GPX)
			$Caches = RegExCount("<type>Geocache",$GPX)
			MSGOK msg="Successfully exported $Caches Geocaches and $wpts Child Waypoints to file: $GPXFilename.gpx $_NewLine $POIMessage $FoundPOIMessage $PlacedPOIMessage $AttributePOIMessage"
		ENDIF
		EXITSUB
	ENDIF

	$count = At($trigger,$GPX)

	WHILE $count > 0
		$count = $count -4
		WHILE SubStr($GPX,$count,4)<>"<wpt" AND $count >= 0
			$count = $count - 1
		ENDWHILE

		#DEBUG status=on

		IF $count <0
			EXITSUB
		ENDIF

		$GPXHeader = SubStr($GPX,1,$count - 1)
		$GPXFooter = SubStr($GPX,$count,0)
		$Wpt = Extract($GPXFooter,"</wpt>",1) + "</wpt>"
		$GPXFooter = Right( $GPXFooter,Len($GPXFooter) - Len($Wpt))
		$Wpts = $Wpts + 1
		GOSUB name=ParseWaypoint
		$GPX = $GPXHeader + $Wpt + $GPXFooter
		$count = At($trigger,$GPX)
	ENDWHILE

	$result = PutFile($GPXFile, $GPX)
	IF Left($result, 7) = "*Error*"
		# If theres any error abort the macro
		CANCEL Msg="Unable to write to $file!"
	ENDIF

	$Wpts = $Wpts + RegExCount("<type>Waypoint",$GPX)
	$Caches = RegExCount("<type>Geocache",$GPX)

	IF $Silent = FALSE
		# Get specific counts of geocaches and child waypoints
		MSGOK msg="Successfully exported $Caches Geocaches and $wpts Child Waypoints to file: $GPXFilename.gpx $_NewLine $POIMessage $FoundPOIMessage $PlacedPOIMessage  $AttributePOIMessage"
	ENDIF

ENDSUB #name=ConvertWaypoints


