		GOSUB name=RestoreFilter
		MFILTER Expression=$d_HasTravelBug Join=AND
		GOSUB name=DoAttributePOIExport
	ELSE
		GOSUB name=EraseOldAttributePOI
	ENDIF
	
	$AttributeTypeName = "Attributes"
