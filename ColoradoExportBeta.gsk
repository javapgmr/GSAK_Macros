BEGINSUB Name=AutoDetect

	# Searching for the Colorado
	# Use the new GPSInfo("GarminPath") function which returns path to GarminDevice.xml

	$result=GPSInfo("GarminPath")

	$Model = "GPS"

	IF FileExists($result)
		$Drive = Extract($result,":",1)
		$Model = GPSInfo("GarminModel")
		$SWversion = GPSInfo("GarminVersion")
		$SWver = Val($SWversion) / 100
		IF $Silent = FALSE
			MSGOK msg="Found $Model running software version $SWver on Drive $Drive:\"
		ENDIF
	ELSE
		MSGOK msg="GPSr not detected - please check connection or enter file path manually"
		$Drive = $Drivestored
	ENDIF
	
	IF $Model = "Oregon 550" OR $Model = "Oregon 550t" OR $Model = "Oregon 450" OR $Model = "Oregon 450t"
		$TotalMaxCaches = 5000
	ELSE
		$TotalMaxCaches = 2000
	ENDIF

ENDSUB Name=AutoDetect

BEGINSUB Name=DisplayForm

	############################################################
	# Form to Collect / Adjust User Input
	############################################################

# Auto Run?

IF $ExportSettings <> ""
	# Load settings passed in $ExportSettings and auto run
	$SaveBox = $ExportSettings
	GOSUB name=SaveBox
	
ELSE

	$Form = Editform($Form,"VersionLabel","caption","$Revision")
	$Form = Replace("%%GPS%%",$Model,$Form)

	WHILE TRUE
		IF $UseCurrent
			$Form = EditForm($Form,"Database","Enabled","No")
			$Form = EditForm($Form,"Filter","Enabled","No")
			$Form = EditForm($Form,"LocationName","Enabled","No")
			$Form = EditForm($Form,"DatabaseLabel","Enabled","No")
			$Form = EditForm($Form,"FilterLabel","Enabled","No")
			$Form = EditForm($Form,"LocationLabel","Enabled","No")
		ELSE
			$Form = EditForm($Form,"Database","Enabled","Yes")
			$Form = EditForm($Form,"Filter","Enabled","Yes")
			$Form = EditForm($Form,"LocationName","Enabled","Yes")
			$Form = EditForm($Form,"DatabaseLabel","Enabled","Yes")
			$Form = EditForm($Form,"FilterLabel","Enabled","Yes")
			$Form = EditForm($Form,"LocationLabel","Enabled","Yes")
		ENDIF
				
		IF $AttributeAsPoi
			$Form = EditForm($Form,"AttribParking","Enabled","Yes")
			$Form = EditForm($Form,"AttribCorrected","Enabled","Yes")
			$Form = EditForm($Form,"AttribDNF","Enabled","Yes")
			$Form = EditForm($Form,"AttribEarthcache","Enabled","Yes")
			$Form = EditForm($Form,"AttribMicro","Enabled","Yes")
			$Form = EditForm($Form,"AttribUnavailable","Enabled","Yes")
			$Form = EditForm($Form,"AttribGCAttributes","Enabled","Yes")
			$Form = EditForm($Form,"AttribBug","Enabled","Yes")
		ELSE
			$Form = EditForm($Form,"AttribParking","Enabled","No")
			$Form = EditForm($Form,"AttribCorrected","Enabled","No")
			$Form = EditForm($Form,"AttribDNF","Enabled","No")
			$Form = EditForm($Form,"AttribEarthcache","Enabled","No")
			$Form = EditForm($Form,"AttribMicro","Enabled","No")
			$Form = EditForm($Form,"AttribUnavailable","Enabled","No")
			$Form = EditForm($Form,"AttribGCAttributes","Enabled","No")
			$Form = EditForm($Form,"AttribBug","Enabled","No")
		ENDIF
		
		IF $_Version <= "7.7.0.0"
			$Form = EditForm($Form,"GCAttributes","Enabled","No")
			$Form = EditForm($Form,"AttribGCAttributes","Enabled","No")
			$AttribGCAttributes = FALSE
		ENDIF
				
		IF $UseCustomName
			$Form = EditForm($Form,"CustomName","Enabled","Yes")
		ELSE
			$Form = EditForm($Form,"CustomName","Enabled","No")
		ENDIF
		
		IF $UseCustomFoundName
			$Form = EditForm($Form,"CustomFoundName","Enabled","Yes")
		ELSE
			$Form = EditForm($Form,"CustomFoundName","Enabled","No")
		ENDIF
		
		IF $UseCustomPlacedName
			$Form = EditForm($Form,"CustomPlacedName","Enabled","Yes")
		ELSE
			$Form = EditForm($Form,"CustomPlacedName","Enabled","No")
		ENDIF
	
		$result = Form($form,"")
		# Reset the screen position of the menu form to where the user left it
		$Form = EditForm($Form,"ColoradoExport","Top",$_FormTop)
		$Form = EditForm($Form,"ColoradoExport","Left",$_FormLeft)
		BEGINCASE #Buttons
		CASE $Cancel
			RETURN
		CASE $Save
			GOSUB name=Save
		CASE $Delete
			GOSUB name=Delete
		CASE $result="SaveBox"
			GOSUB name=SaveBox
		CASE $GCAttributes
			GOSUB name=SelectAttributes
			GOSUB name=PackFilterData
		CASE $result = "AttributeAsPoi"
			# Toggle Sub boxes for attributes as POIs
		CASE $result = "UseCurrent"
			# Toggle disabling filter selection
		CASE $result = "UseCustomName"
			# Toggle Use Custom Name box
		CASE $result = "UseCustomFoundName"
			# Toggle Use Custom Found Name box
		CASE $result = "UseCustomPlacedName"
			# Toggle Use Custom Placed Name box
		CASE $OK
			BREAK
		OTHERWISE
			RETURN
		ENDCASE

	ENDWHILE # Form Loop
	
ENDIF

	$MaxCache = Val($MaxCacheStr)
	$NameSQL = "... Last active"
	GOSUB name=PackFilterData
	GOSUB name=UpdateDataFile

	# Save current database
	$currentdb = $_CurrentDatabase
	$OldCenter = Sysinfo("Center")

	IF $UseCurrent = FALSE

		# Change database if necessary
		IF $_CurrentDatabase <> $Database
			DATABASE Name="$Database" Action=Select
		ENDIF

		# Apply the location
		IF $OldCenter <> $LocationName
			CENTRE location=$LocationName
		ENDIF

		# Apply the Filter
		IF $Filter = "NONE"
			MFILTER Expression=TRUE
		ELSE
			FILTER Name=$Filter
		ENDIF

	ENDIF

	IF $Dbasfilename
		$GPXFilename = $_CurrentDatabase
	ENDIF

	$GPXPath = $Drive + ":\Garmin\GPX\"

	#$GPXFile = $GPXPath + $GPXFilename + ".gpx"
	
	$ExportCount = $MaxCache

	IF $MaxCache > $TotalMaxCaches
		$Form2 = Replace("%%max%%","$TotalMaxCaches",$Form2)
		WHILE True # Infinite loop to redisplay form as required
			$FormExit = form($form2,"")
			BEGINCASE
			CASE $FormExit = "DelayExit"
				#MSGOK msg="Form exit via Delay"
				BREAK
			CASE $FormExit = "SystemExit"
				#MSGOK msg="Form exit via red X button on form"
				RETURN
			CASE $FormExit = "Cancel"
				#MSGOK msg="Form exit from the cancel button"
				RETURN
			CASE $FormExit = "ExportAll"
				BREAK
			CASE $FormExit = "OK"
				# Set the count = $TotalMax Caches
				$ExportCount = $TotalMaxCaches
				BREAK
			ENDCASE
		ENDWHILE
		
	ENDIF

	# Save current Filter
	#$SavedFilter = SaveFilter()
	GOSUB name=SaveFilter

ENDSUB Name=DisplayForm

# Save values
BEGINSUB name=Save
	IF $SaveBox = $PromptSaveText
		$Name = ""
	ELSE
		$Name = $SaveBox
	ENDIF
	$result = Form($form3,"")
	IF $NameOK
		$NameSQL = Replace("'","''",$Name)
		GOSUB name=UpdateDataFile
		$_sql = "SELECT SaveName FROM SavedSettings ORDER BY SaveName"
		#$SaveNameValues = $PromptSaveText + ";" +  Sqlite("sql",$_sql)
		$SaveNameValues = Sqlite("sql",$_sql)
		$SaveNameValues = Replace($_NewLine,";",$SaveNameValues)
		$SaveBoxList = $SaveNameValues
		$SaveBox = $Name
	ENDIF
ENDSUB

# Delete values
BEGINSUB name=DELETE
	IF $SaveBox = $PromptSaveText
		EXITSUB
	ENDIF
	$Name = $SaveBox
	$Form4 = EditForm($Form4,"FilterLabel","Caption",$Name)
	$result = Form($form4,"")
	IF $DeleteOK
		$NameSQL = Replace("'","''",$Name)
		$_sql = "DELETE FROM SavedSettings WHERE SaveName = '$NameSQL'"
		$status = Sqlite("sql",$_sql)
		$_sql = "SELECT SaveName FROM SavedSettings ORDER BY SaveName"
		#$SaveNameValues = $PromptSaveText + ";" +  Sqlite("sql",$_sql)
		$SaveNameValues = Sqlite("sql",$_sql)
		$SaveNameValues = Replace($_NewLine,";",$SaveNameValues)
		$SaveBoxList = $SaveNameValues
		$NameSQL = "... Last active"
		$SaveBox = $NameSQL
		GOSUB name=SaveBox
	ENDIF
ENDSUB

# Retrieve Saved value
BEGINSUB Name=SaveBox
	$SaveBoxSQL = Replace("'","''",$SaveBox)
	$_sql = "SELECT SaveText FROM SavedSettings WHERE SaveName = '$SaveBoxSQL'"
	$result = Sqlite("sql",$_sql,"delim=%%")
	IF Len($result) > 0
		$variables = $result
		GOSUB name=parse
	ENDIF
ENDSUB

BEGINSUB name=SaveFilter
	$SQL = DbToSQLite("caches","code",$datafile,"")
ENDSUB #SaveFilter

BEGINSUB name=RestoreFilter
	TRANSACTION action=begin
	CANCELFILTER
	$match = sqlite("sql","select code from caches")
	MACROFLAG Type=Clear range=all
	$result = CodeMatch($match,$_NewLine,"M")
	MFILTER Expression=$d_MacroFlag
	TRANSACTION action=end
ENDSUB #RestoreFilter

#Subroutine to update datafile
BEGINSUB name=UpdateDataFile
	$variables = "$Drive;$Database;$Filter;$LocationName;$UseCurrent;$Silent;$IncludeChild;$MaxCacheStr;$LogCount;$MyLogs;$GPXFilename;$Dbasfilename;$BenchCache;$GSAKCache;$CacheIDCount;$IncludeUserNotes;"
	$variables = $variables + "$ChildAsPoi;$FoundAsPOI;$FoundDatabase;$FoundFilter;$PlacedAsPOI;$PlacedDatabase;$PlacedFilter;$UseCustomName;$CustomName;$FoundChildPOI;$PlacedChildPOI;$UseBigCharacters;"
	$variables = $variables + "$UseCustomFoundName;$CustomFoundName;$UseCustomPlacedName;$CustomPlacedName;$AttributeAsPoi;$AttribParking;$AttribCorrected;$AttribDNF;$AttribEarthcache;$AttribMicro;$AttribUnavailable;"
	$variables = $variables + "$AttribGCAttributes;$SavedFilterData;$AttribBug"
	$variablesSQL = Replace("'","''",$variables)
	$_sql = "REPLACE INTO SavedSettings VALUES ('$NameSQL','$variablesSQL')"
	$status = Sqlite("sql",$_sql)
	
	$_sql = "REPLACE INTO SavedSettings VALUES ('GSAK_IDCount','$CacheIDCount')"
	$status = Sqlite("sql",$_sql)
ENDSUB Name=UpdateDataFile

BEGINSUB Name=GPXExport

	# Restore Filter
	#$dummy = RestoreFilter($SavedFilter,True)
	GOSUB name=RestoreFilter

	IF $_Count = 0
		MSGOK msg="There are no caches in your selected combination of database and filter $_newline                                  Exiting macro"
		RETURN
	ENDIF
	
	# Open the SQLite database to allow interrogation of the child waypoints
	$sqlinputfile = SqlQuote($_CurrentDataPath + "\sqlite.db3")
	$_sql = "ATTACH $sqlinputfile AS gsakdb"
	$status = Sqlite("sql",$_sql)
	
	# Write out Child description macro
	$tmpS = PutFile($_Install + "\macros\ChildDescription.gsk", $ChildDescription)

	# Now a MACROSET to ensure particular options on the GPX export
	# with dynamic overrides for user-defined Settings
	$GPXdynamic = $GPXstatic + "chkMyLogs.Checked=$MyLogs" + $_NewLine
	$GPXdynamic = $GPXdynamic + "cbxLimit.Text=$LogCount" + $_NewLine
	$GPXdynamic = $GPXdynamic + "chkChild.Checked=$IncludeChild" + $_NewLine
	$GPXDynamic = $GPXdynamic + "cbxUserNotes.Checked=$IncludeUserNotes" + $_NewLine
	
	IF $UseCustomName
		$GPXDynamic = $GPXdynamic + "edtFormat.Text=" + $CustomName + $_NewLine
		# Strip off just the %children portion
		#$tmpS = Extract($CustomName, "%" + "children",2)
		$tmpS = RegExSub(".*?%" + "children(.*)",$CustomName,1,1)
		IF $tmpS = ""
			$GPXDynamic = $GPXdynamic + "edtWaypoint.Text=%" + "code" + $_NewLine
		ELSE
			$GPXDynamic = $GPXdynamic + "edtWaypoint.Text=%" + "code%" + "children" + $tmpS + $_NewLine
		ENDIF
		$GPXDynamic = $GPXdynamic + "chkApplyName.Checked=True" + $_NewLine
	ELSE
		$GPXDynamic = $GPXdynamic + "edtFormat.Text=%" + "Name" + $_NewLine
		$GPXDynamic = $GPXdynamic + "edtWaypoint.Text=%" + "code" + $_NewLine
		$GPXDynamic = $GPXdynamic + "chkApplyName.Checked=False" + $_NewLine
	ENDIF

	
	# Either use the same file name each time
	
	$GPXFile = $GPXPath + $GPXFilename + ".gpx"
	
	
	
