   //-----add markers to the map (Clusters on/off)-----//
   function addMarker(thisMarker,thisInfo,thisName,thisNumber) {

      clusterer.AddMarker(thisMarker, thisName);
      
      GEvent.addListener(thisMarker, "mouseover", function(){
//         alert("mouseover: " + thisNumber);
      });
      
      
      GEvent.addListener(thisMarker, "click", function(){
         map.closeInfoWindow();
         thisInfo="<div style='width: 270px; height: 265px; top:-5px'>" + thisInfo + "</div>"
         var infoTabs = [
            new GInfoWindowTab("Cache", thisInfo),
//            new GInfoWindowTab("Waypoints",'<div id="miniMap" class = "list"></div>')
            new GInfoWindowTab("Waypoints",'<div id="miniMap"></div>')
         ];
         thisMarker.openInfoWindowTabsHtml(infoTabs);

         //-----miniMap to show detail & waypoints-----//
         if (geoCache[thisNumber][6] == "found") {
            var miniMarker = new GMarker(new GPoint(geoCache[thisNumber][3], geoCache[thisNumber][2]), {icon:eval(geoCache[thisNumber][4]+"_found_icon"), title:geoCache[thisNumber][1]});
         }     
         else {
            var miniMarker = new GMarker(new GPoint(geoCache[thisNumber][3], geoCache[thisNumber][2]), {icon:eval(geoCache[thisNumber][4]+"_icon"), title:geoCache[thisNumber][1]});
         }
         var minimapDiv = document.getElementById("miniMap");
         var minimap = new GMap2(minimapDiv);
         var miniBounds = new GLatLngBounds();

         minimap.addControl(
            new GSmallZoomControl(),
            new GControlPosition(G_ANCHOR_TOP_LEFT, new GSize(2,2))); 
         minimap.setCenter(miniMarker.getPoint(), 15);
         minimap.addOverlay(miniMarker);
         miniBounds.extend(miniMarker.getPoint());
         
           
         //---trap map type change and remember for next minimMap-----//
         GEvent.addListener(minimap, 'maptypechanged', function(){
            if (minimap.getCurrentMapType() == G_SATELLITE_MAP){
               miniMapType = "Satellite";
           }

           if (minimap.getCurrentMapType() == G_NORMAL_MAP){
              miniMapType = "Map";
           }

           if (minimap.getCurrentMapType() == G_HYBRID_MAP){
              miniMapType = "Hybrid";
           }
         }); 

         //-----child waypoint array structure-----//
         //childWaypoint[n][0] - Code
         //childWaypoint[n][1] - Name
         //childWaypoint[n][2] - Type
         //childWaypoint[n][3] - Lat
         //childWaypoint[n][4] - Lon

         for (var n=0; n < children +1; n++) {
            if (childWaypoint[n][0] == geoCache[thisNumber][0]) {
               switch (childWaypoint[n][2]) {
                  case "Parking Area":
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("parking_icon"), title:childWaypoint[n][1]});
                  break;
                  case "Trailhead":
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("trailhead_icon"), title:childWaypoint[n][1]});
                  break;
                  case "Reference Point":
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("reference_icon"), title:childWaypoint[n][1]});
                  break;
                  case "Final Location":
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("final_icon"), title:childWaypoint[n][1]});
                  break;
                  case "Question to Answer":
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("question_icon"), title:childWaypoint[n][1]});
                  break;
                  case "Stages of a Multicache":
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("multistage_icon"), title:childWaypoint[n][1]});
                  break;
                  default:
                     var wayMarker = new GMarker(new GPoint(childWaypoint[n][4], childWaypoint[n][3]), {icon:eval("trailhead_icon"), title:childWaypoint[n][1]});
                  break;
               }
               minimap.addOverlay(wayMarker);
               miniBounds.extend(wayMarker.getPoint());
            }
         }
         minimap.addControl(
            new GMapTypeControl(1),
            new GControlPosition(G_ANCHOR_TOP_RIGHT, new GSize(7,2))
         ); 
         var miniZoom = minimap.getBoundsZoomLevel(miniBounds)
         miniCenter = miniBounds.getCenter();
         //avoid zooming too far in
         if (miniZoom > 16){
            minimap.setZoom(16);
         }
         else {
            minimap.setZoom(miniZoom  - 1);
         }
         minimap.setCenter(miniCenter);

         //-----Set the miniMapType same as last used MapType-----//
         if (miniMapType == "Satellite"){
            minimap.setMapType(G_SATELLITE_TYPE);
         }

         if (miniMapType == "Map"){
            minimap.setMapType(G_NORMAL_MAP);
         }

         if (miniMapType == "Hybrid"){
            minimap.setMapType(G_HYBRID_MAP);
         }
      });
      bounds.extend(thisMarker.getPoint());
      return thisMarker;
   }




