   //-----Build infoWindows-----/
   function buildInfoWindows() {

      //---geoCache Array Structure---//
      //geoCache[n][0]  - code
      //geoCache[n][1]  - name
      //geoCache[n][2]  - lat
      //geoCache[n][3]  - lon
      //geoCache[n][4]  - type
      //geoCache[n][5]  - owner
      //geoCache[n][6]  - found?
      //geoCache[n][7]  - tb?
      //geoCache[n][8]  - difficulty
      //geoCache[n][9]  - terrain
      //geoCache[n][10] - container
      //geoCache[n][11] - geoCalc
      //geoCache[n][12] - label
      //geoCache[n][13] - URL
      //geoCache[n][14] - children
      //geoCache[n][15] - available

      for (var n=0; n < caches; n++) {

          if(geoCache[n][15] == "No"){
            cacheName = "<font color='red'><strike>" + geoCache[n][1] + "</strike></font>"
          }
          else {
            cacheName = geoCache[n][1]
          }
         var GSAKpage= "<a href=\gsak://%FF/search/" + geoCache[n][0] +  "/" + my_database + "\>" + "View GSAK" + "</a>";
         var GSAKflag= "<a href=\gsak://%FF/flagtoggle/" + geoCache[n][0] + "/" + my_database + "\>" + "GSAK User Flag" + "</a>";
         cacheInfo  = "<font size = 2.0 face = Tahoma><center><a href='"
         cacheInfo += geoCache[n][13] + "' target='geo'>"      //url
         cacheInfo += geoCache[n][0] + "</a><br><b>";          //code
//         cacheInfo += geoCache[n][1] + "<br></b><i>by: ";      //name
         cacheInfo += cacheName + "<br></b><i>by: ";      //name
         cacheInfo += geoCache[n][5] + "</i><br><br>";         //owner
         cacheInfo += eval(geoCache[n][4] + "_image");         //type
         cacheInfo += eval(geoCache[n][7] + "_image") + "<br><br>Difficulty: ";     //tb
         cacheInfo += "<img src='" + myGME_Library + "img_stars" + geoCache[n][8] + ".gif'><br>Terrain: ";   //difficulty
         cacheInfo += "<img src='" + myGME_Library + "img_stars" + geoCache[n][9] + ".gif'><br>Size: ";      //terrain
         cacheInfo += geoCache[n][10] + "<br><br>";            //container size
         cacheInfo += eval(geoCache[n][6]);                    //found
         cacheInfo += geoCache[n][11]  + "<br><br><font size = 1.8>";           //geoCalc coordinates
         cacheInfo += zoom_In + " | " + zoom_Out + " | ";      //zoom
         cacheInfo += GSAKpage + " | " + GSAKflag;             //URI Support
         cacheInfo += "</font></center>";   

         if (geoCache[n][6] == "found") {
            thisCache[n] = new GMarker(new GPoint(geoCache[n][3], geoCache[n][2]), {icon:eval(geoCache[n][4]+"_found_icon"), title:geoCache[n][1]});
         }
         else {
            thisCache[n] = new GMarker(new GPoint(geoCache[n][3], geoCache[n][2]), {icon:eval(geoCache[n][4]+"_icon"), title:geoCache[n][1]});
         }
         addMarker(thisCache[n],cacheInfo,geoCache[n][1],n);
      }
   }

