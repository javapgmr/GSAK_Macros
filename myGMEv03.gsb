BeginSub Name=InitializeVars
   $myGME_Library = replace("\","/",$myGME_Library,true)
   $maxRadius = 0    #initialize the variable

   if $myfilter <> "Current GSAK Grid"
      if $myFilter <> "None"
         FILTER Name=$myFilter
      else
         CANCELFILTER
      endif
   endif

   #-----sort the collection-----#
   sort by="$mySort"

   #-----start at first record-----#
   $thisRecord = -1
   $cacheList = ""
   $Child="No"
   $Children = ""
   $qtyChildren=-1
   goto position=top

   $testCenter=Sysinfo("gsakini;LastCenter;Lat")

   if $optCollection .or. $testCenter= ""
   #----initialize center coordinates of the collection-----#
      if $optCorrected
         #---Corrected Coords---#
         $max_lat = val($d_latitude)
         $max_lon = val($d_longitude)
         $min_lat = val($d_latitude)
         $min_lon = val($d_longitude)
      else
         #---Original Coords---#
         $max_lat = val($d_latOriginal)
         $max_lon = val($d_lonOriginal)
         $min_lat = val($d_latOriginal)
         $min_lon = val($d_lonOriginal)
      endif
   endif
   
EndSub  # InitializeVars

#############################

BeginSub Name=ProcessCollection
   #-----loop thru the collection-----#
   while .not. $_EOL
      $thisRecord = $thisRecord + 1
      $message = "Now processing: " + "$thisRecord" + " of " + "$_Count"
      ShowStatus msg="$message"

      if $d_distance > $maxRadius
         $maxRadius = $d_distance
      endif

   if $optCorrected
      if $optCollection .or. $testCenter=""
         #-----determine center coordinates of collection-----#
         if val($d_latitude) > $max_lat
            $max_lat = val($d_latitude)
         else
            if val($d_latitude) < $min_lat        
               $min_lat = val($d_latitude)
            endif
         endif

         if val($d_longitude) > $max_lon
            $max_lon = val($d_longitude)
         else
            if val($d_longitude) < $min_lon 
               $min_lon = val($d_longitude)
            endif
         endif
      endif
   else
      if $optCollection .or. $testCenter=""
         #-----determine center coordinates of collection-----#
         if val($d_latOriginal) > $max_lat
            $max_lat = val($d_latOriginal)
         else
            if val($d_latOriginal) < $min_lat        
               $min_lat = val($d_latOriginal)
            endif
         endif

         if val($d_lonOriginal) > $max_lon
            $max_lon = val($d_lonOriginal)
         else
            if val($d_lonOriginal) < $min_lon 
               $min_lon = val($d_lonOriginal)
            endif
         endif
      endif
   endif

      #-----remove bad characters-----#
      $validchars = "[°€{}\w\s\x21-\x25\x27-\x2E\xBA-\xFF]*"

      $cacheName = $d_Name 
      $count = RegExCount($validChars, $cacheName)
      $cleaned = ""
      $int = 1
      while $int <= $count
         $good = RegExData($validChars,$cacheName,$int)
         $cleaned = $cleaned + $good
         $int = $int + 1
      endwhile
      $cacheName = replace($_Quote,"'",$cleaned,true)
      $ownerName = replace($_Quote,"'",$d_OwnerName,true)

      $Label = eval($txtExpression)
      $Label = replace(" ","&nbsp;",$Label,true)    #keep expression in a single line, no wrapping at spaces

      #-----apply the correct difficulty & terrain graphics-----#
      $s_diff = str($d_difficulty,3,1) 
      $s_diff = replace(",",".",$s_diff,true)      # Some regions use [,] decimal separator vs [.]
      $s_diff = replace(".0","",$s_diff,true)
      $s_diff = replace(".5","_5",$s_diff,true)

      $s_terr = str($d_terrain,3,1) 
      $s_terr = replace(",",".",$s_terr,true)      # Some regions [,] decimal separator vs [.]
      $s_terr = replace(".0","",$s_terr,true)
      $s_terr = replace(".5","_5",$s_terr,true)

      #-----determine cache type-----#
      # Key   Cache Type    
      # ====  ============  
      # "B"   LetterBox     
      # "C"   CITO          
      # "E"   Event         
      # "G"   Benchmark     
      # "L"   Locationless  
      # "M"   Multi         
      # "O"   Other         
      # "R"   Earth         
      # "T"   Traditional   
      # "U"   Unknown       
      # "V"   Virtual       
      # "W"   Webcam        
      # "A"   Ape        
     
      $cacheType = $d_CacheType
      
      $cacheTypeKeys = "_BCEGLMORTUVWA"
      $cacheTypeDesc = "other;letterbox;cito;event;benchmark;locationless;multi;other;earth;traditional;unknown;virtual;webcam;ape;"
      $index = at($cacheType,$cacheTypeKeys)
      $thisCacheType = Extract($cacheTypeDesc, ";", $index)

      #-----geoCalc function-----#
      if $optOriginal
         $LatLon = GeoCalc(($d_LatOriginal + "; " + $d_LonOriginal),"FormatMinutes")
      else
         $LatLon = GeoCalc(($d_Latitude + "; " + $d_Longitude),"FormatMinutes")
      endif
      
      #-----found | not found-----#
      if $d_found 
         $s_found ="found"
         $cacheIcon = $thisCacheType  + "_found_icon" 
      else
         $s_found ="notFound"
         $cacheIcon = $thisCacheType + "_icon"
      endif

      #-----TB | no TB-----#
      if $d_HasTravelBug
         $bug = "TB"   
      else
         $bug = "noTB"
      endif
      
      #-----available-----#
      if $d_archived .or. $d_tempDisabled
         $avail = "No"
      else
         $avail = "Yes"
      endif

      #-----build the geoCache list for arrayLoader()-----#
      if $optCorrected
         $cacheList = $cacheList + "geoCache[" + "$thisRecord" + "]=[" + Quote($d_code) + "," +  Quote($cacheName) +  "," + $d_latitude +  ","  + $d_longitude + ","  + Quote($thisCacheType) + "," + Quote($ownerName) + "," + Quote($s_Found) + "," + Quote($bug) + "," + Quote($s_diff)  + "," + Quote($s_terr) + "," + Quote($d_Container) + "," +  Quote("(" + $LatLon + ")") + "," + Quote($Label)  + "," + Quote($d_URL)  
      else
         $cacheList = $cacheList + "geoCache[" + "$thisRecord" + "]=[" + Quote($d_code) + "," +  Quote($cacheName) +  "," + $d_latOriginal +  ","  + $d_lonOriginal + ","  + Quote($thisCacheType) + "," + Quote($ownerName) + "," + Quote($s_Found) + "," + Quote($bug) + "," + Quote($s_diff)  + "," + Quote($s_terr) + "," + Quote($d_Container) + "," +  Quote("(" + $LatLon + ")")  + "," + Quote($Label) + "," + Quote($d_URL)  
      endif

      #-----Child Waypoints-----#
      table active=waypoints scope=parent
      while not ($_eol)
         $Child = "Yes"
         $qtyChildren = $qtyChildren +1
         $s_cName = replace($_Quote,"&#034",$d_cName,true)    #Replace Quotes (")

         $children = $children + "childWaypoint[" + "$qtyChildren" + "]=[" + Quote($d_cParent) + "," + Quote($s_cName) + "," + Quote($d_cType) + "," + Quote($d_cLat) + "," + Quote($d_cLon) + "];" +  $_newLine
         goto position=next
      endwhile

      #-----finish the cacheList string-----#
      $cacheList = $cacheList +  "," + Quote($Child) + "," + Quote($avail) + "];" + $_newLine

      $Child = "No"
      table active=caches
      goto position=next
   endwhile
   
   $thisRecord = $thisRecord + 1   # account for the last record
   $message = "Now processing: " + "$thisRecord" + " of " + "$_Count"
   ShowStatus msg="$message"

   if $myRadius = "Max"
      $myRadius = "$maxRadius"
   endif

   #-----number of records in this collection-----#
   $myCaches = NumtoStr($_Count)         
   
   #-----compute the center coordinates & optimal zoom-----#
   if $optCenterpoint and .not. ($testCenter ="")
      $center_lat = allTrim(Sysinfo("gsakini;LastCenter;Lat"))
      $center_lon = allTrim(Sysinfo("gsakini;LastCenter;Lon"))
   else
      $center_lat = allTrim(NumToStr(($max_lat + $min_lat)/2))
      $center_lon = allTrim(NumToStr(($max_lon + $min_lon)/2))
   endif

EndSub # ProcessCollection

BeginSub Name=ProcessHTMLFile
   #-----convert "," to "." for some regions-----#
   $center_lat = replace(",",".",$center_lat,true) 
   $center_lon = replace(",",".",$center_lon,true) 

   ShowStatus msg="Building the html file..."

   #---- finish the $cacheList string--------------#
   $cacheList = $header + $cacheList  + $_newLine
