#-----Save the config info from the form-----#
BeginSub Name=SaveConfigData

   $strConfig = "#-----myGME Config Variables-----#" + $_newLine
   $strConfig = $strConfig + "#" + $_newLine 
   $strConfig = $strConfig + "$" + "myVersion = " + quote($myVersion) + ";                        # myGME Version " + $_newLine
   $strConfig = $strConfig + "#" + $_newLine 

   if $chkSilent
      $strConfig = $strConfig + "$" + "runSilently = " + quote("True") + ";                     # Run without User Options form" + $_newLine
   else
      $strConfig = $strConfig + "$" + "runSilently = " + quote("False") + ";                     # Run with User Options form" + $_newLine
   endif

   $strConfig = $strConfig + "#" + $_newLine 
   $strConfig = $strConfig + "$" + "myRadius = " + quote($distRadius) + ";                           # Radius" + $_newLine
   if $optCustom
      $strConfig = $strConfig + "$" + "myMarkers = " + quote("Custom") + ";                     # Marker Type" + $_newLine
   endif
   if $optGoogleMaps
      $strConfig = $strConfig + "$" + "myMarkers = " + quote("GoogleMaps") + ";                  # Marker Type" + $_newLine
   endif
   if $optMyGME
      $strConfig = $strConfig + "$" + "myMarkers = " + quote("myGME") + ";                      # Marker Type" + $_newLine
   endif
   if $optMiles 
      $strConfig = $strConfig + "$" + "myUOM = " + quote("mi") + ";                             # UOM:  Mi/km"  + $_newLine
   else
      $strConfig = $strConfig + "$" + "myUOM = " + quote("km") + ";                             # UOM:  Mi/km"  + $_newLine
   endif   
   $strConfig = $strConfig + "$" + "myColor = " + quote($cmbStyle) + ";                       # Color Scheme" + $_newLine
   if $optSortName
      $strConfig = $strConfig + "$" + "mySort = " + quote("name") + ";                          # Sort by:  Name/Code/Distance" + $_newLine
   endif
   if $optSortCode
      $strConfig = $strConfig + "$" + "mySort = " + quote("code") + ";                          # Sort by:  Name/Code/Distance" + $_newLine
   endif
   if $optSortDistance
      $strConfig = $strConfig + "$" + "mySort = " + quote("distance") + ";                     # Sort by:  Name/Code/Distance" + $_newLine
   endif
   if $optClustersOn
      $strConfig = $strConfig + "$" + "myClusters = " + quote("On") + ";                        # Clusters On/Off" + $_newLine
   else
      $strConfig = $strConfig + "$" + "myClusters = " + quote("Off") + ";                       # Clusters On/Off" + $_newLine
   endif
   if $optMenusOn
      $strConfig = $strConfig + "$" + "myMenu = " + quote("Show") + ";                          # Menus Expanded/Collapsed" + $_newLine
   else
      $strConfig = $strConfig + "$" + "myMenu = " + quote("Hide") + ";                          # Menus Expanded/Collapsed" + $_newLine
   endif

   $strConfig = $strConfig + "#" + $_newLine 
   if $optCenterpoint
      $strConfig = $strConfig + "$" + "myCenter = " + quote("Centerpoint") + ";                 # Map Center" + $_newLine
   else
      $strConfig = $strConfig + "$" + "myCenter = " + quote("Collection") + ";                  # Map Center" + $_newLine
   endif

   if $optOriginal
      $strConfig = $strConfig + "$" + "myCoordinates = " + quote("Original") + ";               # Marker Coordinates" + $_newLine
   else
      $strConfig = $strConfig + "$" + "myCoordinates = " + quote("Corrected") + ";              # Marker Coordinates" + $_newLine
   endif

   if $optNumbers
      $strConfig = $strConfig + "$" + "myLabels = " + quote("Numbers") + ";                     # Index Label type" + $_newLine
      $strConfig = $strConfig + "$" + "myExpression = " + quote($txtExpression) + ";             # Special Index Label Expression" + $_newLine
   else
      $strConfig = $strConfig + "$" + "myLabels = " + quote("Expression") + ";                     # Index Label Type" + $_newLine
      $strConfig = $strConfig + "$" + "myExpression = " + quote($txtExpression) + ";             # Index Label Expression" + $_newLine
   endif

   $strConfig = $strConfig + "$" + "myFilter = " + quote($cmbFilters) + ";       # Saved GSAK Filter" + $_newLine
   
   #Custom Markers
   $strConfig = $strConfig + "#" + $_newLine 
   $strConfig = $strConfig + "$" + "c_iconSize = " + quote($c_iconSize) + ";                   # Custom Markers - icon size (w x h)" + $_newLine
   $strConfig = $strConfig + "$" + "c_shadowSize = " + quote($c_shadowSize) + ";                 # Custom Markers - shadow size (w x h)" + $_newLine
   $strConfig = $strConfig + "$" + "c_iconAnchor = " + quote($c_iconAnchor) + ";                 # Custom Markers - anchor location" + $_newLine
   $strConfig = $strConfig + "$" + "c_infoWindowAnchor = " + quote($c_infoWindowAnchor) + ";           # Custom Markers - infoWindow anchor" + $_newLine
   
   #Custom Waypoint Markers
   $strConfig = $strConfig + "#" + $_newLine 
   $strConfig = $strConfig + "$" + "c_wp_iconSize = " + quote($c_wp_iconSize) + ";                # Custom Waypoint Markers - icon size (w x h)" + $_newLine
   $strConfig = $strConfig + "$" + "c_wp_shadowSize = " + quote($c_wp_shadowSize) + ";              # Custom Waypoint Markers - shadow size (w x h)" + $_newLine
   $strConfig = $strConfig + "$" + "c_wp_iconAnchor = " + quote($c_wp_iconAnchor) + ";               # Custom Waypoint Markers - anchor" + $_newLine
   $strConfig = $strConfig + "$" + "c_wp_infoWindowAnchor = " + quote($c_wp_infoWindowAnchor) + ";       # Custom Waypoint Markers - infoWindow anchor" + $_newLine

   #Hosted Google Maps
   $strConfig = $strConfig + "#" + $_newLine 
   $strConfig = $strConfig + "$" + "myTitle = " + quote($txtTitle) + ";      # Map Title" + $_newLine
   $strConfig = $strConfig + "$" + "myKey = " + quote($txtGoogleAPI) + ";      # Google API Key" + $_newLine
   $strConfig = $strConfig + "$" + "myHost = " + quote($txtHost) + ";      # Host Location" + $_newLine
   
   $strConfig = $strConfig + "#" + $_newLine 

   $strConfig = $strConfig + "#-----End-----#" + $_newLine 
   
   SET $Data = PutFile($configFile,$strConfig)
   gosub Name=ReadConfigFile
  
EndSub  #SaveConfigData

