#-----Read the variables from the form-----#

BEGINSUB Name=ReadFormVars

   $myRadius = $distRadius
   $myFilter = $cmbFilters
   $myColor = $cmbStyle
   $myKey = $txtGoogleAPI
   $myHost = $txtHost

   #-----Google Hues-----#
   #FFFA73 - Yellow
   #99B3CC - Blue
   #A7CC95 - Green
   #F2BF24 - Orange
   #D1D0CD - Grey

   if $myColor = "Yellow"
      $myColor = "#FFFA73"
   endif
   if $myColor = "Blue"
      $myColor = "#99B3CC"
   endif
   if $myColor = "Green"
      $myColor = "#A7CC95"
   endif
   if $myColor = "Orange"
      $myColor = "#F2BF24"
   endif
   if $myColor = "Grey"
      $myColor = "#D1D0CD"
   endif
      
  $launch_google_maps = "Y"

   if $optKilometers
      $myUOM = "km"
   endif
   if $optMiles
      $myUOM = "mi"
   endif

   if $optSortName
      $mySort = "name"
   endif

   if $optSortCode
      $mySort = "code"
   endif

   if $optSortDistance
      $mySort = "distance"
   endif

   if $optClustersOn
      $myClusters = "On"
   endif

   if $optClustersOff
      $myClusters = "Off"
   endif

   if $optMenusOn 
      $myMenu = "show"
   endif

   if $optMenusOff
      $myMenu = "hide"
   endif

   if $optCustom
      $myMarkers = "Custom"
   endif

   if $optGoogleMaps
      $myMarkers = "GoogleMaps"
   endif

   if $optMyGME
      $myMarkers = "myGME"
   endif

   if $optCenterpoint
      $myCenter = "Centerpoint"
   endif

   if $optCollection
      $myCenter = "Collection"
   endif

   if $optOriginal
      $myCoordinates = "Original"
   endif

   if $optCorrected
      $myCoordinates = "Corrected"
   endif

   if $optNumbers
      $myLabels = "Numbers"
   endif

   if $optExpression
      $myLabels = "Expression"
      $myExpression = $txtExpression
      if $myExpression = ""
         $myExpression = "%" + "shortname"
