verCheck Version=7.1.1.00 (You can find the latest version of GSAK in the forums at http://support.gsak.net/.)
gosub Name=DeclareVariables

$myVersion = "03i"

$myFilename = $_install + "\macros\myGMEv3.html"
$hostFilename = $_install + "\macros\myGMEv3_hosted.html"
$myGME_Library = $_install + "\macros\myGME_Library\"

gosub Name=CreateDirectory
gosub Name=ReadConfigFile

if $VersionChange
   $runSilently = false
   gosub Name=saveConfigData
endif
if .not. $runSilently
   gosub Name=BuildForm
endif
gosub Name=ValidateFiles
gosub Name=InitializeVars
gosub Name=ProcessCollection
gosub Name=ProcessHTMLFile
gosub Name=LaunchGoogleMaps

#-----create directory and files as appropriate-----#
BeginSub Name=CreateDirectory
   $batFile = $_Install + "\babel.bat" 
   $dirLibrary = "myGME_Library"
   $batString = "cd Macros" + chr(13) + chr(10) + "md " + $dirLibrary
   Set $Data = PutFile($batFile,$batString)

   $dirLibrary=$_Install + "\Macros\" + $dirLibrary

   #-----Create myGME_Library Directory-----#
   if .not. folderExists($dirLibrary)
      FileOpen File="$batFile" Wait=Yes   
   endif

   if .not. folderExists($dirLibrary)
      Cancel msg=myGME could not create the required directory. $_newLine $_newLine "$dirLibrary" $_newLine $_newLine Macro cancelled
   endif

   #-----Create User Config File-----#
   $configFile = $myGME_Library + "myConfig.txt"

   if .not. (FileExists($configFile))
      $image = MimeDecode($myConfig)
      $result = PutFile($configFile,$image)
   endif
EndSub  #CreateDirectory

#-----ReadConfigFile & Populate Form-----#
BeginSub Name=ReadConfigFile

   #-----Set default values if missing from config file-----#
   $chkSilent = false                        # Run without forms

   $cmbStyle = "Yellow"                      # Default Color = "Yellow"
   $distRadius = "5"                         # Default Radius = 5
   $optMiles = true                          # Default "mi"
   $optSortName = true                       # Default Sort by "name"
   $optClustersOn = true                     # Deafult Clusters "On"
   $optMyGME = true                          # Default myGME Markers
   $optMenusOn = true                        # Default Menus "Show"
   $cmbFilters  = "Current GSAK Grid"        # Default Fiter

   $optCenterpoint = True                    # Center of the Map & Radius
   $optCorrected = True                      # Use corrected coordinates
   $optOriginal = False                      # Use corrected coordinates
   $optNumbers = True                        # Index Numbers for labels
   $txtExpression = "<b>%" + "shortname=15" +  "</b><br>%" + "dif/%" + "ter/%" + "con"   # Label expression
   
   #Custom Markers
   $c_iconSize = "(27,34)"                   # Custom Markers - icon size (w x h)
   $c_shadowSize = "(38,33)"                 # Custom Markers - shadow size (w x h)
   $c_iconAnchor = "(6, 35)"                 # Custom Markers - anchor location
   $c_infoWindowAnchor = "(18,10)"           # Custom Markers - infoWindow anchor

   #Custom Waypoint Markers
   $c_wp_iconSize = "(27,34)"                # Custom Waypoint Markers - icon size (w x h)
   $c_wp_shadowSize = "(38,33)"              # Custom Waypoint Markers - shadow size (w x h)
   $c_wp_iconAnchor = "(8,35)"               # Custom Waypoint Markers - anchor
   $c_wp_infoWindowAnchor = "(18, 10)"       # Custom Waypoint Markers - infoWindow anchor

   #Hosted Google Maps
   $txtGoogleAPI = "n/a"                     # Default Google Maps API "n/a"
   $txtHost = "n/a"                          # Default Google Maps API "n/a"

   $radiusList = "0;2;5;10;15;25;Max"
   $colorList = "Yellow;Green;Blue;Orange;Grey"
   $filterList = "Current GSAK Grid;None;" + SysInfo("Filters")
   $expressionList= "Smartname;Code"

   FileRead File=$configFile
   
      #-----read each line and parse for variable and value-----#
      $len = at(";","$line")
      $str = allTrim(SubStr("$line",1,$len -2))
      $myVariable = allTrim(SubStr("$str",1,at("=","$str")-1))
      $myValue = SubStr($str,at("=","$line")+3,$len)
   
      #-----populate form from config.txt-----#
      if $myVariable = "$" +"myVersion"
         if $myValue = $myVersion
            #same version - do nothing
            $UserOptions = editform($UserOptions,"lblVersion","Caption","myGMEv" + $myVersion)
         else
            #-----New version - do something-----# 
            $VersionChange = True
         endif
      endif

      if $myVariable = "$" + "runSilently"
         if $myValue = "True"
            $chkSilent = true
         else
            $chkSilent = false
         endif
         $runSilently = $chkSilent
         if $VersionChange
            $runSilently = False
         endif
      endif

      if $myVariable = "$" + "myRadius"
         $distRadius = $myValue
         $myRadius = $myValue
      endif

      if $myVariable = "$" + "myMarkers"
         if $myValue = "Custom"
            $optCustom = true
            $optGoogleMaps = false
            $optMyGME = false
         else
            if $myValue = "GoogleMaps"
                $optGoogleMaps = true
                $optMyGME = false
                $optCustom = false
            else
                $optMyGME = true
            $optCustom = false
            $optGoogleMaps = false
            endif
            $optMiles = true
         endif
         $myMarkers = $myValue
      endif

      if $myVariable = "$" + "myUOM"
         if $myValue = "km"
            $optKilometers = true
            $optMiles = false
         else
            $optMiles = true
            $optKilometers = false
         endif
         $myUOM = $myValue
      endif
   
      if $myVariable = "$" + "myColor"
         $cmbStyle = $myValue
         if $myValue = "Yellow"
            $myColor = "#FFFA73"
         endif
         if $myValue = "Blue"
            $myColor = "#99B3CC"
         endif
         if $myValue = "Green"
            $myColor = "#A7CC95"
         endif
         if $myValue = "Orange"
            $myColor = "#F2BF24"
         endif
         if $myValue = "Grey"
            $myColor = "#D1D0CD"
         endif
      endif
   
      if $myVariable = "$" + "mySort"
         if $myValue = "code"
            $optSortCode = true
            $optSortName = false
            $optSortDistance = false
         else
            if $myValue = "distance"
               $optSortDistance = true
               $optSortName = false
               $optSortCode = false
            else
               #default "name"
               $optSortName = true
               $optSortCode = false
               $optSortDistance = false
            endif
         endif
         $mySort = $myValue
      endif

      if $myVariable = "$" + "myClusters"
         if $myValue = "On"
            $optClustersOn = true
            $optClustersOff = false            
         else
            $optClustersOff = true
            $optClustersOn = false
         endif
         $myClusters = $myValue
      endif
   
      if $myVariable = "$" + "myCenter"
         if $myValue = "Collection"
            $optCenterpoint = false
            $optCollection = true            
         else
            $optCollection = false
            $optCenterpoint = true
         endif
         $myCenter = $myValue
      endif

      if $myVariable = "$" + "myCoordinates"
         if $myValue = "Original"
            $optOriginal = true
            $optCorrected = false            
         else
            $optCorrected = true
            $optOriginal = false
         endif
         #myCoordinates = $myValue
      endif

      if $myVariable = "$" + "myMenu"
         if $myValue = "Hide"
            $optMenusOff = true
            $optMenusOn = false
         else
            $optMenusOff = false
            $optMenusOn = true
         endif
         $myMenu = $myValue
      endif

      if $myVariable = "$" + "myLabels"
         if $myValue = "Numbers"
            $optNumbers = true
            $optExpression = false
         else
            $optNumbers = false
            $optExpression = true
         endif
         $myLabels = $myValue
      endif

      if $myVariable = "$" + "myExpression"
         $txtExpression = $myValue
         $myExpression = $myValue
      endif

      if $myVariable = "$" + "myFilter"
         $cmbFilters = $myValue
         $myFilter = $myValue
      endif
   
      if $myVariable = "$" + "myKey"
         $txtGoogleAPI = $myValue
         $myKey = $myValue
      endif

      if $myVariable = "$" + "myHost"
         $txtHost = $myValue
         $myHost = $myValue
      endif

      if $myVariable = "$" + "myTitle"
         $txtTitle = $myValue
         $myTitle = $myValue
      endif

      if $myVariable = "$" + "c_iconSize"
         $c_iconSize = $myValue
      endif
      
      if $myVariable = "$" + "c_shadowSize"
         $c_shadowSize = $myValue
      endif

      if $myVariable = "$" + "c_iconAnchor"
         $c_iconAnchor = $myValue
      endif

      if $myVariable = "$" + "c_infoWindowAnchor"
         $c_infoWindowAnchor = $myValue
      endif

      if $myVariable = "$" + "c_wp_iconSize"
         $c_wp_iconSize = $myValue
      endif

      if $myVariable = "$" + "c_wp_shadowSize"
         $c_wp_shadowSize = $myValue
      endif

      if $myVariable = "$"+ "c_wp_iconAnchor"
         $c_wp_iconAnchor = $myValue
      endif

      if $myVariable = "$" + "c_wp_infoWindowAnchor"
         $c_wp_infoWindowAnchor = $myValue
      endif

   EndRead

EndSub #ReadConfigFile

