endif

   #-----Search and replace variables in the final $cacheList string-----#
   $cacheList = replace("my_title",$myTitle,$cacheList,true)
   $cacheList = replace("my_centerLat",$center_lat,$cacheList,true)
   $cacheList = replace("my_centerLon",$center_lon,$cacheList,true)
   $cacheList = replace("my_sort",quote($mySort),$cacheList,true)
   $cacheList = replace("my_radius",$myRadius,$cacheList,true)
   $cacheList = replace("my_uom",quote($myUOM),$cacheList,true)
   $cacheList = replace("my_color",$myColor,$cacheList,true)
   $cacheList = replace("my_key",$myKey,$cacheList,true)
   $cacheList = replace("my_clusters",quote($myClusters),$cacheList,true)
   $cacheList = replace("my_caches",$myCaches,$cacheList,true)
   $cacheList = replace("my_menu",$myMenu,$cacheList,true)
   $cacheList = replace("GSAK_install",quote($_install),$cacheList,true)
   $cacheList = replace("my_children",NumToStr($qtyChildren),$cacheList,true)
   $cacheList = replace("my_database",quote(HttpEncode($_CurrentDatabase)),$cacheList,true)
   $cacheList = replace("my_markers",quote($myMarkers),$cacheList,true)
   $cacheList = replace("my_labels",quote($myLabels),$cacheList,true)

   #-----Custom markers - unique filenames-----#
   if $myMarkers = "Custom"
      $cacheList = replace("mrk_","c_mrk_",$cacheList,true)
      #Custom Markers
      $cacheList = replace("c_iconSize",$c_iconSize,$cacheList,true)
      $cacheList = replace("c_shadowSize",$c_shadowSize,$cacheList,true)
      $cacheList = replace("c_iconAnchor",$c_iconAnchor,$cacheList,true)
      $cacheList = replace("c_infoWindowAnchor",$c_infoWindowAnchor,$cacheList,true)
      #Custom Waypoint Markers
      $cacheList = replace("c_wp_iconSize",$c_wp_iconSize,$cacheList,true)
      $cacheList = replace("c_wp_shadowSize",$c_wp_shadowSize,$cacheList,true)
      $cacheList = replace("c_wp_iconAnchor",$c_wp_iconAnchor,$cacheList,true)
      $cacheList = replace("c_wp_infoWindowAnchor",$c_wp_infoWindowAnchor,$cacheList,true)
   endif
   
   if $myClusters = "On"
      $cacheList = replace("markers_per_cluster","35",$cacheList,true)
#      $cacheList = replace("cluster_status","Clusters [Off]",$cacheList,true)
      $cacheList = replace("cluster_status","Clusters",$cacheList,true)
   else
      $cacheList = replace("markers_per_cluster",quote($myCaches),$cacheList,true)
#      $cacheList = replace("cluster_status","Clusters [On]",$cacheList,true)
      $cacheList = replace("cluster_status","Clusters",$cacheList,true)
   endif    

   #-----Create the Hosted Cache File-----#
   if $myKey = "n/a" 
      #do nothing
   else
      if $myKey = "" 
         #also do nothing
      else
         if $myHost = "test"
            #do nothing
         else
            if $myHost = ""
               #do nothing
            else
               $hosted_cacheList = $cacheList
               $hosted_cacheList = replace("file:///my_GME_Library",$myHost +"/",$hosted_cacheList,true)
               SET $Data = Putfile($hostFilename,$hosted_cacheList)
            endif
         endif
      endif
   endif

   #-----Create the local Cache File-----#
   $cacheList = replace("my_GME_Library",$myGME_Library,$cacheList,true)

   SET $Data = Putfile($myFilename,$cacheList)

EndSub # ProcessHTMLFile

