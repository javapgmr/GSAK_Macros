#----------------------------------------------------
BEGINSUB Name=iCalFileCreate
#----------------------------------------------------               	
$op=""
$op=$op+"BEGIN:VCALENDAR"+$_NewLine +"VERSION:1.0"+$_NewLine +"BEGIN:VEVENT"+$_NewLine
$op=$op+"DTSTART:"+ DateToString($d_placeddate)+"T090000"+$_NewLine
$op=$op+"DTEND:"  + DateToString($d_placeddate)+"T100000"+$_NewLine
$op=$op+"LOCATION;ENCODING=QUOTED-PRINTABLE:"+   replace(";","  ",GCalc( $d_Latitude + " "+ $d_Longitude, "FormatMinutes")) +$_NewLine
$op=$op+"DESCRIPTION;ENCODING=QUOTED-PRINTABLE:Click the link to open up the cache page for the event http://coord.info/" + $d_Code +$_NewLine
IF $d_placedby=$d_OwnerName 
	$op=$op+"SUMMARY;ENCODING=QUOTED-PRINTABLE:Geocaching Event: "+$d_name+ " by " + $d_PlacedBy +$_NewLine
ELSE
	$op=$op+"SUMMARY;ENCODING=QUOTED-PRINTABLE:Geocaching Event: "+$d_name+ " by " + $d_PlacedBy + " (" + $d_OwnerName+")"+$_NewLine
ENDIF	
$op=$op+"END:VEVENT"+$_NewLine +"END:VCALENDAR"+$_NewLine

#save the ics file
$result=PutFile($icsFile, $op)
IF Left($result, 7) = "*Error*"
   CANCEL Msg="Unable to save the .ics file: $icsFile!"
ENDIF

ENDSUB 


#----------------------------------------------------
BEGINSUB Name=Configure
#----------------------------------------------------
WHILE TRUE # Infinite loop to display main form until exit conditions are chosen
	IF $rbnGoogle
        $frmConfigure = EditForm($frmConfigure,"chkOpenCal","enabled","Yes")
        $frmConfigure = EditForm($frmConfigure,"chkFPathtoClip","enabled","Yes")
  ELSE
      	$frmConfigure = EditForm($frmConfigure,"chkOpenCal","enabled","No")
        $frmConfigure = EditForm($frmConfigure,"chkFPathtoClip","enabled","No")
      	
  ENDIF	
  
	 $FormExit = Form($frmConfigure,"")
   BEGINCASE    		
       CASE $FormExit = "btn2Run" 
        	  GOSUB Name=SaveDatFile
            BREAK
            
        CASE $FormExit = "btnUpdate"    
	           GOSUB Name=CheckUpdates           
              
        CASE ($FormExit = "btn2Cancel") OR ($FormExit = "SystemExit") 
        	  GOSUB Name=SaveDatFile            
            CANCEL  
            
        CASE $FormExit="btnAbout"
             GOSUB Name="DisplayAbout"   
                          
   ENDCASE  
   
ENDWHILE
ENDSUB

#----------------------------------------------------
BEGINSUB Name=Add2Outlook
#----------------------------------------------------
#add info to user notes for the event
$d_UserNote=REPLACE("BBBB","Outlook",REPLACE("AAAA",substr($_Time,1,5) + " on " + DateFormat($_Today),$Note1))+$d_UserNote
  
#run the .ics file to create the apointment.
FILEOPEN File="$icsFile" Wait=No
ENDSUB


#----------------------------------------------------
BEGINSUB Name=Add2Google
#----------------------------------------------------
#add info to user notes for the event
$d_UserNote=REPLACE("BBBB","Google",REPLACE("AAAA",substr($_Time,1,5) + " on " + DateFormat($_Today),$Note1))+$d_UserNote
  
IF $chkFPathtoClip
	CLIP data="$icsFile"
ENDIF	

IF $chkOpenCal
	 WEB URL="https://www.google.com/calendar?tab=mc"
ENDIF
ENDSUB


