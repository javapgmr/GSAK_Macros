# Version check for ver 7.7.0.0 or greater
VERCHECK Version=7.7.0.0 (You can find the latest version of GSAK at http://gsak.net)

GOSUB Name=MacroName                #Check macro name ends in gsk or txt
GOSUB Name=DeclareVariables         #Declare all the variables
GOSUB Name=Initalise Variables      #kind of obvious what this sub does!
GOSUB Name=GetVer                   #set $CurrentVersion from MacVersion
GOSUB Name=CheckDatFile             #check datfile exists and create if not
GOSUB Name=CheckForUpdates          #check macro version if option set
       
#set form caption
$form = EditForm($form,"Form1","Caption","iCal Entry generator:  v" + $CurrentVersion)

WHILE TRUE # Infinite loop to display main form until exit conditions are chosen
	 $FormExit = Form($form,"")
   BEGINCASE    		
        CASE $_CtrlKey = TRUE
            GOSUB Name="Configure"
            BREAK
          
        CASE $FormExit = "DelayExit"
            BREAK            
              
        CASE $FormExit = "SystemExit"
            CANCEL                          
   ENDCASE     
ENDWHILE
    

#check to see if current waypoint is an event
IF RegExCount($d_CacheType, "CEXZ") =0
	MSGOK msg="This is not an event so a calendar entry has not been created."  Caption="Not an Event Cache"
	CANCEL
ENDIF

#Generate iCal file
GOSUB Name=iCalFileCreate
	
#check to see if the current waypoint is has already been added to the User Notes i.e. its already been exported
$Ok2Proceed=FALSE
IF RegExCount("iCal\.gsk",$d_UserNote) > 0
	IF YesNo(" This event has already been exported. $_newline $_newline Are you sure you want to do this?","Event previously added" )
	   $Ok2Proceed=TRUE
	ENDIF
ELSE
	$Ok2Proceed=TRUE
ENDIF
	

IF $Ok2Proceed
	BEGINCASE    		
      CASE $rbnGoogle = TRUE
          GOSUB Name=Add2Google
          BREAK
        
      CASE $rbnOutlook = TRUE
      	  GOSUB Name=Add2Outlook
          BREAK            
                     
   ENDCASE  
ENDIF
   



