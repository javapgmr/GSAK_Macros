    array(3,2) = BoolToStr($rbtInNotes, "T", "F")
    
    While True
        $ScExit = Form($SectionForm, "")
        $SectionForm = EditForm($SectionForm, "SectionForm", "Top", $_FormTop)
        $SectionForm = EditForm($SectionForm, "SectionForm", "Left", $_FormLeft)
        
        BeginCase
            Case $ScExit = "SystemExit"
                $rbtDontMove = StrToBool(array(3,1))
                $rbtInNotes = StrToBool(array(3,2))
               Break
               
            Case $ScExit = "DelayExit"
                Break
                
            Case $ScExit = "ScOk"
                Break
                
            Case $ScExit = "ScCancel"
                $rbtDontMove = StrToBool(array(3,1))
                $rbtInNotes = StrToBool(array(3,2))
                Break
                
            Case $ScExit = "ScHelp"
                GoSub Name=SectionHelpSub
        EndCase
    EndWhile
EndSub

