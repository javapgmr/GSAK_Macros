#@++++++++++++++++++++
#@ Main processing loop
#@++++++++++++++++++++

BeginSub Name=Main
	# If we get to here then the user must have clicked on the OK button
	# update from form 

    # Select the database If it isn't already
    $currentdb = $_CurrentDatabase
    If $currentdb <> $cbxDataBase
        Database Name="$cbxDataBase" Action=Select
    EndIf

    # Run cm2gpx.exe to create $findsgpx
    RunPgm Pgm=$fnmCm2GPX Parms=$edtCm2options -o "$findsgpx" "$fnmpdbfile" Wait=Yes
    # Make sure findsgpx exists before continuing
    If NOT (FileExists($findsgpx))
        Cancel Msg="FAILURE: $findsgpx does not exist! "
    EndIf

    # Make the CachemateLog dialog from static and dynamic parts
    array(1,1) = $CacheMateLog

    # Code/log
    If $rbtMatchCode
        array(1,2) = $CodeDym
    Else 
        array(1,2) = $SmartDym
    EndIf
    
    # Log/Notes
    If $rbtInLog
        array(1,3) = $LogDym
    Else
        array(1,3) = $NotesDym
    EndIf
    
    # Import $findsgpx into GSAK using user settings If they exist; Else use defaults
    $tmp =  array(1,-1)
    MacroSet Dialog=CachemateLog VarName=$tmp #$tmp
    CacheMate Settings=<macro> File="$findsgpx"
    # If we updated the Logs then auto-log otherwise we are done
    If $rbtInLog
        # Filter so our list is only new finds using the UserFlag set during Cachemate import
        Mfilter Expression If=$d_UserFlag
        # If there's records in the filter, sort 'em and log 'em, pausing between each for the user to confirm.
        If $_FilterCount > 0
            Sort By="FoundByMe;UserSort"
            While NOT ($_EOL)
                Clip Tags="%UserLog"	
                If (NOT($rbtNoTimestamp))
                    GoSub Name=InsertTimestamp
                EndIf
                If $d_found
                    BeginCase
                        Case $d_CacheType="G"
                        If $chkDNFRemove and $d_DNFDate <> [00000000]
                            SETCOLOR Color=clear Row=current Column="name"
                        EndIf
                        #If logging a benchmark, dIfferent url string
                            $url = "http://www.geocaching.com/mark/log.aspx?WP=$d_Code&LogType=2"
 
                        # CITO, Event, Maze or Mega types, Attend Record
                        Case $d_CacheType="C" OR $d_CacheType="E" OR $d_CacheType="X" OR $d_CacheType="Z"
                            # logging attended, no color
                            $url = "http://www.geocaching.com/seek/log.aspx?ID=%gcid&LogType=10"

                        #WebCam
                        Case $d_CacheType="W"
                            # photo taken, no color
                            $url = "http://www.geocaching.com/seek/log.aspx?ID=%gcid&LogType=11"
                        
                        # Everything else
                        Otherwise
                            # found it
                            If $chkDNFRemove and $d_DNFDate <> 0
                                SETCOLOR Color=clear Row=current Column="name"
                            EndIf
                            $url = "http://www.geocaching.com/seek/log.aspx?ID=%gcid&LogType=2"
                    EndCase

				ELSE # Must be Not Found
					GoSub name=ReadGPX
                    If $d_CacheType="G"
                        # update DNF flag and date 
                        $d_DNF = TRUE
                        $d_DNFDate = STRINGTODATE($Year+$Month+$Day)
                        # check to see if we should color
                        IF $chkDNFColor
                            SETCOLOR Color=$Color Row=current Column="name"
                        EndIf
                        #If logging a benchmark, different url string
                        $url = "http://www.geocaching.com/mark/log.aspx?WP=$d_Code&LogType=3"
                        # Set macro flag if not found, will remove it from the moves
                        $d_MacroFlag = True
					# Everything else
					Else
						# not found
						$d_DNFDate = STRINGTODATE($Year+$Month+$Day)
						$d_DNF = TRUE
						# check to see if we should color
						If $chkDNFColor
                            SETCOLOR Color=$Color Row=current Column="name"
                        EndIf
						$url = "http://www.geocaching.com/seek/log.aspx?ID=%gcid&LogType=3"
                        # Clear user flag to take it out of the filter so it is not moved
                        $d_MacroFlag = True
                    EndIf
				EndIF
                Web Url=$url
                Pause Msg="Click Continue when logged successfully."
                # Purge no longer needed logs
                Purgenote Type=LogOnly Range=Current
                Goto Position=Next
            Endwhile        
            # see if we have any thing left to work with ... they could all be DNF's
            MFILTER Expression If=$d_MacroFlag Reverse=Yes Join=AND
            If $_FilterCount > 0
                #update logids
                If $chkRFA
                    Macro file="$_Install\Macros\RefreshAllGPX.gsk"
                    # reset filter after LOAD in macro
                    MFILTER Expression If=$d_UserFlag  
                    MFILTER Expression If=$d_MacroFlag Reverse=Yes Join=AND                
                EndIf
                #see If we want to move or copy records
                If not ($rbtDontMove)      	# move/copy records
                    If  $rbtMove          # move
                        $tmp = $MoveRecords + "cbxDestination.Text=" + $cbxMoveDB + $_Newline
                        MacroSet Dialog=MoveCopy VarName=$tmp
                        MoveCopy Settings=<macro>
                    Else				# copy
                        $tmp =  $CopyRecords + "cbxDestination.Text=" + $cbxMoveDB + $_Newline
                        MacroSet Dialog=MoveCopy Varname=$tmp
                        MoveCopy Settings=<macro>
                    EndIf
                EndIf
            Endif
        Else
            Pause Msg="No found caches in filter! Clearing and exiting."
        EndIf
    EndIf
    # Done with processing, restore to what we were and go back to GUI
    GoSub Name=RestoreState
EndSub

#@++++++++++++++++++++++
#@ Main form support routines
#@++++++++++++++++++++++
BeginSub Name=VarCheck
	# Check values are OK
	# Location of cm2gpx.exe
	If NOT((FileExists($fnmCm2GPX)))
		$msg = "cm2gpx.exe not found." + $_NewLine + "Please check path to cm2gpx.exe"
		Pause msg=$msg
		$VarCheck = False
	EndIf
		
	# location of pdb file
	If NOT((FileExists($fnmPDBFile)))
		$inputmsg = "No such PDB file exists!" + $_NewLine + "(At least, change [PALM USERNAME] to match the first six characters of your Palm username; If needed, change the whole string to match the PDB file to use.)" + $_NewLine + $_NewLine + "(My Palm username is 'jholly', so my entry here reads 'C:\Program Files\Palm\jholly\Backup\DefaultItems-cMat.pdb'.)"
		Pause msg=$inputmsg
		$VarCheck = False
	EndIf

	# If update logid's was chosen, check for macro
	If $chkRFA
		$tmp = $_Install + "\Macros\RefreshAllGPX.gsk"
		If NOT((FileExists($tmp)))
			$msg = "Macrofile for RefreshAllGPX not found" + $_NewLine + "Please install file or uncheck selection"
			Pause msg=$msg
			$VarCheck = False
		EndIf
	EndIf
EndSub

#@++++++++++++++++++++++++++
#@ Date/Time routines
#@++++++++++++++++++++++++++

BeginSub Name=InsertTimestamp
    $ClipText = GetClipText()
    GoSub Name=ReadGPX
    BeginCase
        Case $rbtDateOnly
            GoSub Name=GenerateDate
            
        Case $rbtTimeOnly
            GoSub Name=GenerateTime
            
        Case $rbtDateTime
            GoSub Name=GenerateDate
            $strDate = $strDate + " "
            GoSub Name=GenerateTime
    EndCase
    If $rbtPrepend
        $ClipText = $strDate + $strTime + " " + $ClipText
    Else
        $ClipText = $ClipText + " " + $strDate + $strTime
    EndIf
    CLIP Data=$ClipText
EndSub
    
BeginSub Name=ReadGPX
	$FoundCache = FALSE
	FILEREAD File=$findsgpx
		$line = ALLTRIM($line)
		$token = LEFT($line,6)
		IF $token = "<name>" 
		# extract waypoint name from line
			$end = AT("</", $line)
			$name = SUBSTR($line, 7, $end-7)
			# this the one we are looking for?
			IF $name =  $d_Code
				# yes
				$foundCache = TRUE
			ENDIF
		ENDIF
		# Got to right record, get type
		IF $foundCache = TRUE
			$token = LEFT($line, 6)
			if $token = "<type>"
				# get type of record, last on line
				$type = EXTRACT($line, "|", 3)
				$type = EXTRACT($type, "<", 1)
			ENDIF
		ENDIF 
		# Get date
		IF $foundCache = TRUE
			$token = LEFT($line, 16)
			if $token = "<cmlog:end_time>"
 				$tmp = EXTRACT($line, ">", 2)                
                $ISODate = SubStr($tmp, 1, 10)
                GoSub Name=ConvertDate
                $Time = SubStr($tmp, 12, 5)
                GoSub Name=ConvertTime
				$foundCache = FALSE     # got what we came for, reset for next one and quit 
				STOPREAD
			Endif
		ENDIF
	ENDREAD
ENDSUB

BeginSub Name=ConvertDate
    $Year = SubStr($ISODate, 1, 4)
    $Month = SubStr($ISODate,6,2)
    $Day = SubStr($ISODate,9,2)
EndSub

BeginSub Name=ConvertTime
    $Hour = SubStr($Time,1,2)
    $Minute = SubStr($Time,4,2)
EndSub

BeginSub Name=GenerateDate
    BeginCase
        Case $rbtMMDDYY
            $strDate = $Month + "/" + $Day + "/" + $Year

        Case $rbtDDMMYY
            $strDate = $Day + "/" + $Month + "/" + $Year
          
        Case $rbtISODate
            $strDate = $ISODate
    EndCase
EndSub

BeginSub Name=GenerateTime
    $Suffix = ""
    If $rbt12Hour
        BeginCase
            Case (val($Hour) = 0)
                $Hour = "12"
                $Suffix = "am"
                
            Case ((Val($Hour) > 0) AND (Val($Hour) < 12))
                $Suffix = "am"
                
            Case (Val($Hour) = 12)
                $Suffix = "pm"
                
            Case (val($Hour) > 12)
                $Hour = NumToStr(Val($Hour) - 12)
                $Suffix = "pm"
        EndCase
    # No need to convert to 24, that is what is on the palm
    EndIf 
    $strTime = $Hour + ":" + $Minute + $Suffix
EndSub

