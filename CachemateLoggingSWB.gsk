VerCheck Version=7.5.0.181 (see http://www.gsak.net/board for version 7.5)

GoSub Name=Init

# Clear all user flags and init variables
UserFlag Type=clear Range=All
MacroFlag Type=clear Range=All
$VarCheck = True
array(1,0) = "5"    #array to store cachemate load dialog
array(2,0) = "2"    #array to store code/smart state
array(3,0) = "2"    #array to store log/notes state
array(4,0) = "3"    #array to store  DNF state info
array(5,0) = "2"    #array to store cm2gpx state info

