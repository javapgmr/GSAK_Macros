		ELSE
			$FoundDatabase = Extract($variables,";",19)
		ENDIF

		IF Extract($variables,";",20) = ""
			$FoundFilter = "Found"
		ELSE
			$FoundFilter = Extract($variables,";",20)
		ENDIF

		IF Extract($variables,";",21) = ""
			$PlacedAsPoi = FALSE
		ELSE
			$PlacedAsPoi = StrToBool(Extract($variables,";",21))
		ENDIF

		IF Extract($variables,";",22) = ""
			$PlacedDatabase = "Placed"
		ELSE
			$PlacedDatabase = Extract($variables,";",22)
		ENDIF

		IF Extract($variables,";",23) = ""
			$PlacedFilter = "Placed"
		ELSE
			$PlacedFilter = Extract($variables,";",23)
		ENDIF
		
		IF Extract($variables,";",24) = ""
			$UseCustomName = FALSE
		ELSE
			$UseCustomName = StrToBool(Extract($variables,";",24))
		ENDIF
		
		IF Extract($variables,";",25) = ""
			$CustomName = "%" + "name"
		ELSE
			$CustomName = Extract($variables,";",25)
		ENDIF
		
		IF Extract($variables,";",26) = ""
			$FoundChildPoi = FALSE
		ELSE
			$FoundChildPoi = StrToBool(Extract($variables,";",26))
		ENDIF
		
		IF Extract($variables,";",27) = ""
			$PlacedChildPOI = FALSE
		ELSE
			$PlacedChildPOI = StrToBool(Extract($variables,";",27))
		ENDIF
		
		IF Extract($variables,";",28) = ""
			$UseBigCharacters = FALSE
		ELSE
