BEGINSUB Name=Initialize
	$datafile = $_Install + "\macros\ColoradoExport.db3"
	
	# Set up the SQL table

	IF NOT(FileExists($datafile))
		$status = Sqlite("create",$datafile)
	ENDIF

	$status = Sqlite("open",$datafile)

	$Data="SaveName text UNIQUE, SaveText text"
	$_sql = "CREATE TABLE IF NOT EXISTS SavedSettings ($Data)"
	$status = Sqlite("sql",$_sql)

	$_sql = "SELECT SaveName FROM SavedSettings ORDER BY SaveName"
	#$SaveNameValues = $PromptSaveText + ";" +  Sqlite("sql",$_sql)
	$SaveNameValues = Sqlite("sql",$_sql)
	$SaveNameValues = Replace($_NewLine,";",$SaveNameValues)
	$SaveBoxList = $SaveNameValues
	
	$PromptSaveText = "... Last active"

	#############################################################
	# PreLoad User variables into Form
	############################################################
	# "Default" location of Input file on Colorado, depending
	# on drive mapping at any given moment

	$DriveStored = "Z"
	$Drive = "Z"
	$Database = "NotFound"
	$Filter="Close"
	$LocationName="Home"
	$UseCurrent=TRUE
	$Silent=FALSE
	#set database dropdown values
	$DBList=Sysinfo("Databases")
	# $DatabasesNone="*None*"  + ";" + $Databases
	$FilterList= "NONE;" + SysInfo("Filters")
	$LocationList=Sysinfo("Locations")
	$PlaceName = RegexReplace(",.*?;",$LocationList,";")
	$ExportCount=0
	$IncludeChild=TRUE
	$ChildAsPoi=FALSE
	$IncludeUserNotes = FALSE
	$MaxCache=1975
	$LogCount="5"
	$MyLogs=TRUE
	$GPXFilename="caches"
	$Dbasfilename = FALSE
	$BenchCache=TRUE
	$GSAKCache=TRUE
	$CacheIDCount = 99000000
	$FoundDatabase = "Found"
	$FoundFilter = "Found"
	$FoundAsPOI = FALSE
	$PlacedDatabase = "Placed"
	$PlacedFilter = "Placed"
	$PlacedAsPOI = FALSE
	$UseCustomName = FALSE
	$CustomName = "%" + "name"
	$FoundChildPoi = FALSE

	#############################################################
	# Settings file

	#$datafilepath = Sysinfo("MacroPath")
	#$DataFile = $datafilepath + "\ColoradoExport.dat"

	$NameSQL = "... Last active"
	$_sql = "SELECT SaveText FROM SavedSettings WHERE SaveName='$NameSQL'"
	$variables = Sqlite("sql",$_sql,"delim=%%")
	
	# Get the cacheID count from a global table
	$_sql = "SELECT SaveText FROM SavedSettings WHERE SaveName='GSAK_IDCount' LIMIT 1"
	$CacheIDCount = Val(Sqlite("sql",$_sql))

	GOSUB name=Parse

ENDSUB Name=Initialize

