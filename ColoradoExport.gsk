BEGINSUB name=Parsewaypoint
	# Data is in $Wpt

	$Wpt = RegExReplace(" *?<groundspeak.*\r?\n?",$Wpt,"")
	$Wpt = RegExReplace(" *?</groundspeak.*\r?\n?",$Wpt,"")
	$Wpt = RegExReplace(" *?<type>.*\r?\n?",$Wpt,"")

	IF (RegExCount("<desc>",$wpt))>0
		$linetemp = RegExData("<desc>.*?</desc>",$wpt,1)
		$wpt = RegExReplace("<desc>",$wpt,"<cmt>")
		$wpt = RegExReplace("</desc>",$wpt,"</cmt>")
		$wpt = RegExReplace("</cmt>",$wpt,"</cmt>" + $_NewLine + $linetemp)
	ENDIF

	IF (RegExCount("<name>",$wpt)) >0
		$name = RegExSub("<name>(.*?)</name>",$wpt,1,1)
		SHOWSTATUS msg="Converting waypoint #: $wpts, $name" Width=350
	ENDIF

	IF (RegExCount("</wpt>",$wpt))>0
		$wpt = RegExReplace("</wpt>",$wpt,$wptfooter + "</wpt>" + $_NewLine)
	ENDIF

ENDSUB #name=parsewaypoint

BEGINSUB name=ConvertCaches

	# Terracaches start at 90,000,000. Benchmarks start at 80,000,000. Manual and GG caches start at 99,000,001. GA caches start at 98,000,001

	#Initialize variables
	$NChar=0
	#Set up strings to check characters
	$Numbers="0123456789"
	$Letters="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
	#Filter for Terracache Prefixes where cache id =1
	MFILTER Expression=(Left($d_code,2)="TC" OR Left($d_code,2)="LC") AND (Trim($d_CacheID)="1" OR IsEmpty($d_CacheID))
	IF $_FilterCount > 0
		#Iterate through the filter
		GOTO Position=Top
		WHILE NOT($_EOL)
			$NewCode=0
			#Check the length of the code
			$Length=Len($d_Code)
			#Strip off the TC or LC and convert to upper case
			$DropCode=Upper(Right($d_code, $Length-2))
			# For Terracaches where of same form as GC cache codes
			#Parse each character and convert alphas to base 36
			$Length=$Length-2
			$Pass=1
			WHILE $Pass<=$Length
				$Character=SubStr($DropCode,$Pass,1)
				IF At($Character, $Numbers)=0
					$NChar=At($Character, $Letters) + 9
				ELSE
					$NChar=Val($Character)
				ENDIF
				$NewCode=(36*$NewCode)+$NChar
				$Pass=$Pass+1
			ENDWHILE
			$NewCode=90000000 + $NewCode
			$d_CacheID = "$NewCode"
			GOTO Position=Next
		ENDWHILE
	ENDIF

	#Filter for GA Prefixes where cache id =99999998
	# GA caches start at 98,000,001
	MFILTER Expression=Left($d_code,2)="GA" AND (Trim($d_CacheID)="99999998" OR IsEmpty($d_CacheID))
	IF $_FilterCount > 0
		#Iterate through the filter
		GOTO Position=Top
		WHILE NOT($_EOL)
			$NewCode=0
			#Check the length of the code
			$Length=Len($d_Code)
			#Strip off the GA
			$DropCode=Right($d_code, $Length-2)
			# For GA caches just add 98000000
			$NewCode=98000000 + Int(Val($DropCode))
			$d_CacheID = "$NewCode"
			GOTO Position=Next
		ENDWHILE
	ENDIF

	#Filter for GG Prefixes where cache id =99999999
	MFILTER Expression=Trim($d_CacheID)="99999999"
	IF $_FilterCount > 0
		#Iterate through the filter
		GOTO Position=Top
		WHILE NOT($_EOL)
			# For GG caches allocate numbers sequentially starting with 99,000,001
			$CacheIDCount = $CacheIDCount + 1
			$d_CacheID = "$CacheIDCount"
			GOTO Position=Next
		ENDWHILE
		$NameSQL = "... Last active"
		GOSUB name=UpdateDataFile
	ENDIF

	IF $BenchCache
		#Filter for Benchmarks where cache id =1 and give them dummy unique id#s so they can load as Geocaches
		MFILTER Expression= $d_CacheType="G" AND (Trim($d_CacheID)="1" OR IsEmpty($d_CacheID))
		IF $_FilterCount > 0
			#Iterate through the filter
			GOTO Position=Top
			WHILE NOT($_EOL)
				# For Benchmarks where code is of the form ZZ9999
				$NewCode = 0
				$Pass=1
				WHILE $Pass<=2
					$Character=SubStr($d_Code,$Pass,1)
					$NChar=At($Character, $Letters) - 1
					$NewCode=(26*$NewCode)+$NChar
					$Pass=$Pass+1
				ENDWHILE
				$NewCode = (10000 * $NewCode) + Val(SubStr($d_Code,3,4))
				$NewCode=80000000 + $NewCode
				$d_CacheID = "$NewCode"
				GOTO Position=Next
			ENDWHILE
		ENDIF
	ENDIF

	IF $GSAKCache
		#Filter for non-benchmark caches where cache id =1 and give them dummy unique id#s so they can load as Geocaches
		MFILTER Expression=NOT($d_CacheType="G") AND (Trim($d_CacheID)="1" OR IsEmpty($d_CacheID))

		IF $_FilterCount > 0
			#Iterate through the filter
			GOTO Position=Top
			WHILE NOT($_EOL)
				# For GSAK created caches allocate numbers sequentially starting with 99,000,001
				$CacheIDCount = $CacheIDCount + 1
				$d_CacheId = "$CacheIDCount"
				GOTO Position=Next
			ENDWHILE
			$NameSQL = "... Last active"
			GOSUB name=UpdateDataFile
		ENDIF
	ENDIF

ENDSUB  #name=ConvertCaches


BEGINSUB name=CombinePOIs
	$x = 0
	WHILE $x < $NumChild # Currently we have 6 waypoint types to iterate through
		$ChildTypeName = Extract($ChildTypes,";",$x + 1)
		$CategoryName = $ChildTypeName
		$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName.*","F")
		GOSUB name=CreateBabelLine
		DELAY MS=1000	
		$x = $x + 1
	ENDWHILE
	
	$ChildTypeName = "DNF"
	$CategoryName = "Att-" + $ChildTypeName
	$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName" + "_Attrib.*","F")
	GOSUB name=CreateBabelLine	
	DELAY MS=1000
	$ChildTypeName = "Corrected"
	$CategoryName = "Att-" + $ChildTypeName
	$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName" + "_Attrib.*","F")
	GOSUB name=CreateBabelLine	
	DELAY MS=1000
	$ChildTypeName = "Earthcache"
	$CategoryName = "Att-" + $ChildTypeName
	$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName" + "_Attrib.*","F")
	GOSUB name=CreateBabelLine	
	DELAY MS=1000
	$ChildTypeName = "Micro"
	$CategoryName = "Att-" + $ChildTypeName
	$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName" + "_Attrib.*","F")
	GOSUB name=CreateBabelLine	
	DELAY MS=1000
	$ChildTypeName = "Parking"
	$CategoryName = "Att-" + $ChildTypeName
	$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName" + "_Attrib.*","F")
	GOSUB name=CreateBabelLine	
	DELAY MS=1000
	$ChildTypeName = "Unavailable"
	$CategoryName = "Att-" + $ChildTypeName
	$POIFiles = Dir($POI_StashPath + "\*_" + "$ChildTypeName" + "_Attrib.*","F")
	GOSUB name=CreateBabelLine	
ENDSUB

BEGINSUB name=CreateBabelLine
	IF Extract($POIFiles,$_NewLine,1) <> ""
		SHOWSTATUS msg="Combining POI Files: $ChildTypeName" Width=350
		$BabelLine = Quote($_ExePath + "\gpsbabel.exe") + " -w -i garmin_gpi"
		$y = 1
		WHILE Extract($POIFiles,$_NewLine,$y) <> ""
			$BabelLine = $BabelLine + " -f " + Quote($POI_StashPath + "\" + Extract($POIFiles,$_NewLine,$y))
			$y = $y + 1
		ENDWHILE

		$BabelLine = $BabelLine + " -o garmin_gpi,category=" + Quote($CategoryName)

		IF FileExists("$BMPPath" + "$ChildTypeName" + ".bmp")
			$BabelLine = $BabelLine + ",bitmap=" + Quote($BMPPath + $ChildTypeName + ".bmp")
		ELSE
			$BabelLine = $BabelLine + ",bitmap=" + Quote($BMPPath + "Reference Point.bmp")
		ENDIF

		$BabelLine = $BabelLine + " -F " + Quote($POIPath + "\" + $ChildTypeName + ".gpi")
		#$BabelLine = $BabelLine + " 2> " + Quote(#_Install + "\result.txt")

		$CombinePOIDynamic = $CombinePOIStatic + "edtBabelOut.Text=" + $BabelLine + $_NewLine

		MACROSET Dialog=Custom VarName=$CombinePOIDynamic

		EXPORT type=CUS Settings=<macro>
	ENDIF
ENDSUB



