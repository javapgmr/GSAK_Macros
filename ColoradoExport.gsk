		ENDIF
		
		IF Extract($variables,";",31) = ""
			$UseCustomPlacedName = FALSE
		ELSE
			$UseCustomPlacedName = StrToBool(Extract($variables,";",31))
		ENDIF
		
		IF Extract($variables,";",32) = ""
			$CustomPlacedName = "%" + "Name by " + "%" +"by " + "%" + "typ1" + "%" + "con1 (" + "%" + "Dif/" + "%" + "Ter)"
		ELSE
			$CustomPlacedName = Extract($variables,";",32)
		ENDIF
		
		IF Extract($variables,";",33) = ""
			$AttributeAsPoi = FALSE
		ELSE
			$AttributeAsPoi = StrToBool(Extract($variables,";",33))
		ENDIF
		
		IF Extract($variables,";",34) = ""
			$AttribParking = FALSE
		ELSE
			$AttribParking = StrToBool(Extract($variables,";",34))
		ENDIF
		
		IF Extract($variables,";",35) = ""
			$AttribCorrected = FALSE
		ELSE
			$AttribCorrected = StrToBool(Extract($variables,";",35))
		ENDIF
		
		IF Extract($variables,";",36) = ""
			$AttribDNF = FALSE
